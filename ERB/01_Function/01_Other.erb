@CONVERT_TIME
#FUNCTIONS
IF TIME:0 == 0
	LOCALS:0 = 昼
ELSE
	LOCALS:0 = 夜
ENDIF

RETURNF LOCALS:0

;-------------------------------------------------
;指定範囲内で数値をインクリメントさせる関数
;-------------------------------------------------
@ROUND_INCREMENT(OPERAND, MINVAL, MAXVAL)
#FUNCTION
#DIM OPERAND
#DIM MINVAL
#DIM MAXVAL
OPERAND ++
SIF OPERAND > MAXVAL
	OPERAND = MINVAL

RETURNF OPERAND