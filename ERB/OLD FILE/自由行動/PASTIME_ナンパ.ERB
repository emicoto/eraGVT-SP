;----------------------------------------------------------------------------------------------------------------------------
;ナンパ判定
;ナンパされるかどうかの判定と補正値計算用
@PASTIME_NANPA, ARG
#DIM NANPA

;ナンパ判定
NANPA = 10 + RAND:10

SIF TALENT:学生 == 1
	NANPA -= 3
SIF TALENT:学生 == 2
	NANPA += 1
SIF TALENT:学生 == 3
	NANPA += 3
SIF TALENT:学生 == 4
	NANPA += 3

SIF TALENT:オトコ > 0
	NANPA -= 20

SIF TALENT:巨乳 > 0
	NANPA += 3

SIF TALENT:小柄 > 0
	NANPA += 3
	
SIF TALENT:女体受容 > 0
	NANPA += 5

SIF TALENT:社交的 > 0
	NANPA += 3
SIF TALENT:母性的 > 0
	NANPA += 3
SIF TALENT:小悪魔 > 0
	NANPA += 3
SIF TALENT:目立ちたがり > 0
	NANPA += 3
SIF TALENT:上品 > 0
	NANPA += 3
SIF TALENT:泣き虫 > 0
	NANPA += 3

SIF TALENT:清純派 > 0
	NANPA += 10
	
IF CFLAG:1 > 0
	SIF TALENT:変身時外見 > 0
		NANPA += 5
ELSE
	SIF TALENT:外見 > 0
		NANPA += 5
ENDIF

SIF TALENT:巻き込まれ体質 > 0
	NANPA += 20
SIF TALENT:人外の美貌 > 0
	NANPA += 40
SIF TALENT:小さな体躯 > 0
	NANPA += 5

IF RAND:100 <= NANPA
	RETURN 1
ENDIF
RETURN 0

