;----------------------------------------------------------------------------------------------------------------------------
;痴漢判定
@PASTIME_CHIKAN,ARG
#DIM KONZATU
#DIM STAND_POS
#DIM TEIKOU
#DIM CHIKAN_POS
#DIM CHIKAN_CHECK
#DIMS 目的地
#DIMS 痴漢してきた相手
LOCAL = 0
TEIKOU = 0

;混雑度設定
IF ARG == 3
	;学校へ行く場合は混雑度高め
	KONZATU = RAND:70 + 30
ELSE
	KONZATU = RAND:95
	;朝の場合は混雑度高め
	SIF TIME < 1
		KONZATU += 5
	;夜の場合は混雑度低め
	SIF TIME > 0
		KONZATU -= 5
ENDIF

IF ARG == 1
	目的地 = 遠出
	PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%は少しばかり遠出する為に電車に乗っている。
ELSEIF ARG == 3
	;小学生の場合
	IF TALENT:801 == 1
		目的地 = 小学校
		PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%は小学校に通う為に電車に乗っている。
	;中学生の場合
	ELSEIF TALENT:801 == 2
		目的地 = 中学校
		PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%は中学校に通う為に電車に乗っている。
	;高校生の場合
	ELSEIF TALENT:801 == 3
		目的地 = 高校
		PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%は高校に通う為に電車に乗っている。
	;大学生の場合
	ELSEIF TALENT:801 == 4
		目的地 = 大学
		PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%は大学に通う為に電車に乗っている。
	ENDIF
ENDIF
IF TIME > 0
	IF KONZATU >= 75
		PRINTFORML 車内は帰宅途上のサラリーマンや学生で満員だ。
	ELSEIF KONZATU < 75 && KONZATU >= 45
		PRINTFORML 車内は帰宅途上のサラリーマンや学生で混雑している。
	ELSEIF KONZATU < 45 && KONZATU >= 15
		PRINTFORML 車内にはまばらに人がいるが座席に座れない事もなさそうだ。
	ELSE
		PRINTFORML 車内はガラガラだ。
	ENDIF
ELSE
	IF KONZATU >= 75
		PRINTFORML 車内は出勤中のサラリーマンや登校中の学生で満員だ。
	ELSEIF KONZATU < 75 && KONZATU >= 45
		PRINTFORML 車内は出勤中のサラリーマンや登校中の学生で混雑している。
	ELSEIF KONZATU < 45 && KONZATU >= 15
		PRINTFORML 車内にはまばらに人がいるが座席に座れない事もなさそうだ。
	ELSE
		PRINTFORML 車内はガラガラだ。
	ENDIF
ENDIF
PRINTW 

;オトコ以外なら追加テキスト
IF TALENT:オトコ <= 0 || FILTER_CHECK_F(5) == 0 
	IF KONZATU >= 45 && RAND:4 == 0
		PRINTFORML 人の流れに乗り車内を進む%PRINT_TRANSCALLNAME(TARGET)%だったが
		PRINTFORML 何かに引っかかりスカートがめくれそうになっている事に気がつき慌ててスカートを抑えつけた。
	ELSEIF KONZATU >= 45 && RAND:3 == 0
		PRINTFORML 車内へ乗り込んだものの%PRINT_TRANSCALLNAME(TARGET)%は濁流の様な流れに逆らえず
		PRINTFORML グイグイと人の流れに押し流されていってしまった。
	ELSEIF KONZATU >= 45
		PRINTFORML 混雑する車内にどうにか入ったもののろくに身動きが取れない上に
		PRINTFORML 周囲から漂うむせ返る様な汗臭さに%PRINT_TRANSCALLNAME(TARGET)%は息苦しさを感じている。
	ENDIF
	PRINTL 
ENDIF

;立ち位置確保
IF KONZATU < 15 || (KONZATU < 45 && RAND:4 == 0)
	PRINTFORML 座席を確保できた%PRINT_TRANSCALLNAME(TARGET)%は座って一息ついた・・・
	STAND_POS = -1
ELSEIF KONZATU >= 15 && RAND:4 == 0
	PRINTFORML なんとか開かない側のドアの前を確保できた%PRINT_TRANSCALLNAME(TARGET)%は一息ついた・・・
	STAND_POS = 1
ELSEIF KONZATU >= 15 && RAND:3 == 0
	PRINTFORML なんとか車両端の壁の前を確保できた%PRINT_TRANSCALLNAME(TARGET)%は一息ついた・・・
	STAND_POS = 2
ELSEIF KONZATU >= 15 && RAND:2 == 0
	PRINTFORML なんとか入口付近の窓の前を確保できた%PRINT_TRANSCALLNAME(TARGET)%は一息ついた・・・
	STAND_POS = 3
ELSE
	PRINTFORML 人混みの真っ只中に落ち着いた%PRINT_TRANSCALLNAME(TARGET)%は電車が動き出すと一息ついた・・・
	STAND_POS = 0
ENDIF

;痴漢判定
CHIKAN_CHECK = 10 + RAND:10

SIF TALENT:学生 == 1
	CHIKAN_CHECK -= 3
SIF TALENT:学生 == 2
	CHIKAN_CHECK += 1
SIF TALENT:学生 == 3
	CHIKAN_CHECK += 3
SIF TALENT:学生 == 4
	CHIKAN_CHECK += 3

SIF TALENT:巨乳 > 0
	CHIKAN_CHECK += 3

SIF TALENT:小柄 > 0
	CHIKAN_CHECK -= 3
	
SIF TALENT:女体受容 > 0
	CHIKAN_CHECK += 2

SIF TALENT:社交的 > 0
	CHIKAN_CHECK += 3
SIF TALENT:母性的 > 0
	CHIKAN_CHECK += 3
SIF TALENT:小悪魔 > 0
	CHIKAN_CHECK += 3
SIF TALENT:目立ちたがり > 0
	CHIKAN_CHECK += 3
SIF TALENT:上品 > 0
	CHIKAN_CHECK += 3
SIF TALENT:泣き虫 > 0
	CHIKAN_CHECK += 3

SIF TALENT:清純派 > 0
	CHIKAN_CHECK += 3
	
IF CFLAG:1 > 0
	SIF TALENT:変身時外見 > 0
		CHIKAN_CHECK += 3
ELSE
	SIF TALENT:外見 > 0
		CHIKAN_CHECK += 3
ENDIF

SIF TALENT:巻き込まれ体質 > 0
	CHIKAN_CHECK += 5
SIF TALENT:人外の美貌 > 0
	CHIKAN_CHECK += 5
SIF TALENT:小さな体躯 > 0
	CHIKAN_CHECK -= 3

LOCAL = 0
IF TALENT:変身時ＴＳ > 0 && CFLAG:1 > 0
	SIF CFLAG:326 >= 10
		LOCAL += 10
	SIF CFLAG:326 < 10
		LOCAL += CFLAG:326
	SIF CFLAG:328 >= 10
		LOCAL += 10
	SIF CFLAG:328 < 10
		LOCAL += CFLAG:328
ELSE
	SIF CFLAG:325 >= 10
		LOCAL += 10
	SIF CFLAG:325 < 10
		LOCAL += CFLAG:325
	SIF CFLAG:327 >= 10
		LOCAL += 10
	SIF CFLAG:327 < 10
		LOCAL += CFLAG:327
ENDIF
CHIKAN_CHECK += LOCAL

;痴漢してきた相手を設定
IF RAND:6 == 0
	痴漢してきた相手 = 脂ぎったオヤジ
ELSEIF RAND:5 == 0
	痴漢してきた相手 = チンピラ
ELSEIF RAND:4 == 0
	痴漢してきた相手 = チャラ男
ELSEIF RAND:3 == 0
	痴漢してきた相手 = ヤクザ
ELSEIF RAND:2 == 0
	痴漢してきた相手 = 紳士
ELSE
	痴漢してきた相手 = 中年オヤジ
ENDIF

;とりあえず暫定で混雑度以下で発生
;最終的には魅了値とか素質とかフィートとか時間とか混雑具合でされるか判定
;痴漢された場合にはそのままターン終了、痴漢回避に成功したら選択した行動
IF (RAND:100 <= CHIKAN_CHECK && (TALENT:オトコ <= 0 || FILTER_CHECK_F(5) == 0)) && KONZATU >= 50 && STAND_POS >= 0
	;痴漢してきた相手の立ち位置
	CHIKAN_POS = RAND:2
	PRINTW 
	IF CHIKAN_POS > 0
		IF RAND:4 == 0
			IF STAND_POS == 0
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%が立ち通していると
			ELSEIF STAND_POS == 1
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%がドアの前に立って景色を眺めていると
			ELSE
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%が窓の前に立って景色を眺めていると
			ENDIF
			PRINTFORML 混雑に押されてきた背後に立つ%痴漢してきた相手%と密着する体勢になってしまった。
			PRINTL 
			PRINTFORML ふとももの辺りにくすぐったさを感じてもしやと思った%PRINT_TRANSCALLNAME(TARGET)%が足元を見ると
			PRINTFORML %痴漢してきた相手%の手が%PRINT_TRANSCALLNAME(TARGET)%のふとももに伸びている様に見える…
		ELSEIF RAND:3 == 0
			IF STAND_POS == 0
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%が立ち通していると
			ELSEIF STAND_POS == 1
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%がドアの前に立って景色を眺めていると
			ELSE
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%が窓の前に立って景色を眺めていると
			ENDIF
			PRINTFORML 混雑に押されてきた背後に立つ%痴漢してきた相手%と密着する体勢になってしまった。
			PRINTL 
			PRINTFORML お尻に何か硬い物が当たる感触がした%PRINT_TRANSCALLNAME(TARGET)%が怪訝な顔で辺りを見渡すと、
			PRINTFORML 背後の%痴漢してきた相手%が股間の膨らみを押し付けている気がする…
		ELSEIF RAND:2 == 0
			IF STAND_POS == 0
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%が立ち通していると
			ELSEIF STAND_POS == 1
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%がドアの前に立って景色を眺めていると
			ELSE
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%が窓の前に立って景色を眺めていると
			ENDIF
			PRINTFORML 混雑に押されてきた背後に立つ%痴漢してきた相手%と密着する体勢になってしまった。
			PRINTL 
			PRINTFORML 突然のお尻を撫でられる感触に%PRINT_TRANSCALLNAME(TARGET)%は小さな悲鳴を上げてしまい慌てて口を抑える。
			PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%が周りを見回すと、背後の%痴漢してきた相手%の手がお尻に伸びている様に見える…
		ELSE
			IF STAND_POS == 0
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%が立ち通していると
			ELSEIF STAND_POS == 1
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%がドアの前に立って景色を眺めていると
			ELSE
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%が窓の前に立って景色を眺めていると
			ENDIF
			PRINTFORML 混雑に押されてきた背後に立つ%痴漢してきた相手%と密着する体勢になってしまった。
			PRINTL 
			PRINTFORML 細い何かがふとももを撫でていき%PRINT_TRANSCALLNAME(TARGET)%は小さな悲鳴を上げてしまい慌てて口を抑える。
			PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%が周りを見回すと、背後の%痴漢してきた相手%の手がふとももを撫でている様に見える…
		ENDIF
	ELSE
		IF RAND:3 == 0
			IF STAND_POS == 0
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%が立ち通していると
			ELSEIF STAND_POS == 1
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%がドアを背にしていると
			ELSE
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%が壁を背にしていると
			ENDIF
			PRINTFORML 混雑に押されてきた%痴漢してきた相手%と密着する体勢になってしまった。
			PRINTL 
			PRINTFORML ふとももの辺りにくすぐったさを感じてもしやと思った%PRINT_TRANSCALLNAME(TARGET)%が足元を見ると
			PRINTFORML %痴漢してきた相手%の手が%PRINT_TRANSCALLNAME(TARGET)%のふとももに伸びている様に見える…
		ELSEIF RAND:2 == 0
			IF STAND_POS == 0
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%が立ち通していると
			ELSEIF STAND_POS == 1
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%がドアを背にしていると
			ELSE
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%が壁を背にしていると
			ENDIF
			PRINTFORML 混雑に押されてきた%痴漢してきた相手%と密着する体勢になってしまった。
			PRINTL 
			PRINTFORML 身体に何か硬い物が当たる感触がした%PRINT_TRANSCALLNAME(TARGET)%は怪訝な顔で辺りを見渡すと、
			PRINTFORML 目の前の%痴漢してきた相手%が股間の膨らみを押し付けている気がする…
		ELSE
			IF STAND_POS == 0
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%が立ち通していると
			ELSEIF STAND_POS == 1
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%がドアを背にしていると
			ELSE
				PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%が壁を背にしていると
			ENDIF
			PRINTFORML 混雑に押されてきた%痴漢してきた相手%と密着する体勢になってしまった。
			PRINTL 
			PRINTFORML 胸を何かに触られている気がした%PRINT_TRANSCALLNAME(TARGET)%が胸に目を向けると
			PRINTFORML %痴漢してきた相手%がスマフォを手にし、その画面を凝視しており
			PRINTFORML 電車が揺れる度にスマフォを持った手が%PRINT_TRANSCALLNAME(TARGET)%の胸に触れている…
		ENDIF
	ENDIF	
	PRINTW 
	PRINTL 
	SIF ABL:欲望 >= 3
		PRINTFORML [0]なすがまま
	SIF ABL:欲望 <= 2
		PRINTFORML [0]我慢する
	PRINTFORML [1]抵抗　　　　（抵抗失敗率{ABL:従順 * 10 + LOCAL + 35}％）
	;PRINTFORML [2]強く抵抗する（抵抗失敗率{ABL:従順 * 10 + 15}％）
	PRINTFORML [2]電車を降りる
	$INPUT_LOOP_CHIKAN
	INPUT
	IF RESULT == 0
		PRINTL 
		IF RAND:3 == 0
			PRINTFORML 自分の気のせいかもしれないと思った%PRINT_TRANSCALLNAME(TARGET)%は
			PRINTFORML 不快感を感じながらも我慢することにした・・・
		ELSEIF RAND:2 == 0
			PRINTFORML すぐにやめてくれるかもしれないと考えた%PRINT_TRANSCALLNAME(TARGET)%は
			PRINTFORML 怯えつつも我慢することにした・・・
		ELSE
			PRINTFORML ここで大事にすると面倒になる。
			PRINTFORML 相手もそれは望まないだろうと考えた%PRINT_TRANSCALLNAME(TARGET)%は
			PRINTFORML 速く目的地に着くことを祈りながら我慢することにした・・・
		ENDIF
		TEIKOU = 1
	ELSEIF RESULT == 1
		PRINTL 
		IF RAND:100 < ABL:従順 * 10 + LOCAL + 35
			IF RAND:3 == 0
				PRINTFORML 抵抗しようとした%PRINT_TRANSCALLNAME(TARGET)%だったが
				PRINTFORML 恐怖に見を竦ませてしまい何も出来ずにいる・・・
			ELSEIF RAND:2 == 0
				PRINTFORML 声を上げようとした%PRINT_TRANSCALLNAME(TARGET)%だったが
				PRINTFORML 口を開いた瞬間、%痴漢してきた相手%に口を抑えられ声を発することができなかった・・・
			ELSE
				PRINTFORML 相手の腕を掴もうとした%PRINT_TRANSCALLNAME(TARGET)%だったが、
				PRINTFORML 逆に自分の腕を%痴漢してきた相手%に掴まれてしまった・・・
			ENDIF
			TEIKOU = 1
		ELSE
			PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%が弱々しく抵抗すると
			PRINTFORML %痴漢してきた相手%は気まずそうに離れていった・・・
			TEIKOU = 0
		ENDIF
	ELSEIF RESULT == 2
		PRINTW 
		PRINTL 
		PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%は最寄りの駅に着くなり逃げ出せるように身構えた…
		PRINTW 
		IF RAND:100 < 25
			IF RAND:4 == 0
				PRINTFORML 電車が停止し扉が開いた瞬間、逃げようとした%PRINT_TRANSCALLNAME(TARGET)%だったが
				PRINTFORML 恐怖に見を竦ませてしまい何も出来ずにいる。
				PRINTFORML 恐怖を振り払おうとするも、気を取り直す前に扉は閉まってしまった・・・
			ELSEIF RAND:3 == 0
				PRINTFORML 電車が停止し扉が開いた瞬間、%PRINT_TRANSCALLNAME(TARGET)%は降りようとしたが
				PRINTFORML しっかり腕を掴まれてしまい動くことができなかった。
				PRINTFORML 引き離そうとするも相手の力に敵わず、悪戦苦闘してる内に扉は閉まってしまった・・・
			ELSEIF RAND:2 == 0
				PRINTFORML 電車が停止し扉が開いた瞬間、逃げようとした%PRINT_TRANSCALLNAME(TARGET)%だったが
				PRINTFORML カラダを押し付けられて身動きが取れなくなってしまった。
				PRINTFORML 相手のカラダを押し退けようとするが、その前に扉は閉まってしまった・・・
			ELSE
				PRINTFORML 電車が停止し扉が開いた瞬間、逃げようとした%PRINT_TRANSCALLNAME(TARGET)%だったが
				PRINTFORML 停車する際に大きく揺れ動き、人混みが流れてきた為更に大きな圧力がかかってしまった。
				PRINTFORML なんとか振り払って出入口に進もうとするが、一歩も進めずに扉は閉まってしまった・・・
			ENDIF
			TEIKOU = 1
		ELSE
			PRINTFORML 電車が停止し扉が開いた瞬間
			PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%は全力で逃げ出した・・・
			TEIKOU = 0
		ENDIF
	ELSE
		GOTO INPUT_LOOP_CHIKAN
	ENDIF
ENDIF
PRINTW 

;抵抗に失敗したら本番開始
IF TEIKOU > 0
	CALL MESSAGE_PASTIME_CHIKAN, ARG, CHIKAN_POS, 痴漢してきた相手
	;持ち帰られていたらターン終了
	SIF RESULT > 0
		RETURN 1
ENDIF

PRINTW 
PRINTW ・・
PRINTW ・・・・
PRINTW ・・・・・・
PRINTL 
RETURN 0

;----------------------------------------------------------------------------------------------------------------------------
;痴漢メッセージ
;通学途中に痴漢された場合、学生証を取られて脅されたりとかありかもしれない
@MESSAGE_PASTIME_CHIKAN, ARG:0, ARG:1, ARGS
;ARG:0		行き先			学校でかつ中学か高生なら制服を着ている描写にしたりとか
;ARG:1		痴漢の立ち位置		相手が自分の前か後ろどちらかチェックして内容分岐
;	0:正面にいる
;	1:背後にいる
;ARGS		痴漢してきた相手	印象による相手の名前表示
#DIM HIP_SIZE
#DIM TAKEOUT

;男女平等のオプション
SIF FILTER_CHECK_F(5) && TALENT:オトコ > 0 
	RETURN 0

;持ち帰り判定
TAKEOUT = 10 + RAND:10

SIF TALENT:学生 == 1
	TAKEOUT -= 3
SIF TALENT:学生 == 2
	TAKEOUT += 1
SIF TALENT:学生 == 3
	TAKEOUT += 3
SIF TALENT:学生 == 4
	TAKEOUT += 3

SIF TALENT:巨乳 > 0
	TAKEOUT += 3

SIF TALENT:小柄 > 0
	TAKEOUT -= 3
	
SIF TALENT:女体受容 > 0
	TAKEOUT += 2

SIF TALENT:社交的 > 0
	TAKEOUT += 3
SIF TALENT:母性的 > 0
	TAKEOUT += 3
SIF TALENT:小悪魔 > 0
	TAKEOUT += 3
SIF TALENT:目立ちたがり > 0
	TAKEOUT += 3
SIF TALENT:上品 > 0
	TAKEOUT += 3
SIF TALENT:泣き虫 > 0
	TAKEOUT += 3

SIF TALENT:清純派 > 0
	TAKEOUT += 3
	
IF CFLAG:1 > 0
	SIF TALENT:変身時外見 > 0
		TAKEOUT += 3
ELSE
	SIF TALENT:外見 > 0
		TAKEOUT += 3
ENDIF

SIF TALENT:巻き込まれ体質 > 0
	TAKEOUT += 5
SIF TALENT:人外の美貌 > 0
	TAKEOUT += 5
SIF TALENT:小さな体躯 > 0
	TAKEOUT -= 3

IF TALENT:変身時ＴＳ > 0 && CFLAG:1 > 0
	SIF CFLAG:326 >= 10
		TAKEOUT += 10
	SIF CFLAG:326 < 10
		TAKEOUT += CFLAG:326
	SIF CFLAG:328 >= 10
		TAKEOUT += 10
	SIF CFLAG:328 < 10
		TAKEOUT += CFLAG:328
ELSE
	SIF CFLAG:325 >= 10
		TAKEOUT += 10
	SIF CFLAG:325 < 10
		TAKEOUT += CFLAG:325
	SIF CFLAG:327 >= 10
		TAKEOUT += 10
	SIF CFLAG:327 < 10
		TAKEOUT += CFLAG:327
ENDIF
TAKEOUT += LOCAL

;外見チェック
SIF CFLAG:1 == 0
	HIP_SIZE = TALENT:外見
SIF CFLAG:1 >= 1
	HIP_SIZE = TALENT:変身時外見

IF ARG:1 > 0
	IF RAND:3 == 0
		PRINTFORML %ARGS%は背後から%PRINT_TRANSCALLNAME(TARGET)%の
		IF TALENT:巨乳 > 2
			PRINT ダイナミックな大きさの
		ELSEIF TALENT:貧乳 == 1
		    PRINT 小振りな
		ELSEIF TALENT:貧乳 == 2
		    PRINT 平坦だが柔らかい
		ELSEIF TALENT:オトコ > 0
		    PRINT 平坦な
		ELSE
			PRINT 豊満な
		ENDIF
		PRINTFORML 胸に手を回し揉みしだき始めた。
	ELSEIF RAND:2 == 0
		PRINTFORML %ARGS%は背後から%PRINT_TRANSCALLNAME(TARGET)%の
		IF HIP_SIZE == 2
			PRINT むちむちとした
		ELSEIF HIP_SIZE == 1
			PRINT 豊満な
		ELSEIF HIP_SIZE == 0
			PRINT 整った
		ELSE
			PRINT 小振りな
		ENDIF
