;********************************************************************************************************************
;CLOTH101 セーラー服
;----------------------------------------------------------
@CLOTH_DESCRIPTION_101
PRINTL  剣に良し。機関銃に良し。戦う乙女のフォーマル。
PRINTL 【 補正 】攻撃1.1倍
PRINTL 【変身時】攻撃1.2倍に効果が上昇
PRINTL 【耐久度】100

;----------------------------------------------------------
;衣装の耐久度
@CLOTH_HP_101
LOCAL = 100
IF CFLAG:1 > 0
	LOCAL += 5 * (FIGURE_SPLIT(EQUIP:101,11) - FIGURE_SPLIT(EQUIP:101,10))
ELSE
	LOCAL += 5 * (FIGURE_SPLIT(EQUIP:1,11) - FIGURE_SPLIT(EQUIP:1,10))
ENDIF
RETURN LOCAL

;----------------------------------------------------------
;補正
@CLOTH_DEF_101
LOCAL = DEFAULT_OUTER_DEF
IF CFLAG:1 > 0
	LOCAL -= 5 * (FIGURE_SPLIT(EQUIP:101,13) - FIGURE_SPLIT(EQUIP:101,12))
ELSE
	LOCAL -= 5 * (FIGURE_SPLIT(EQUIP:1,13) - FIGURE_SPLIT(EQUIP:1,12))
ENDIF
RETURN LOCAL

@CLOTH_HOSEI_KOUGEKI_101
IF CFLAG:1 > 0
	LOCAL = 120
ELSE
	LOCAL = 110
ENDIF
RETURN LOCAL

@CLOTH_HOSEI_HIT_101
LOCAL = 0
IF CFLAG:1 > 0
	LOCAL -= 1 * ((FIGURE_SPLIT(EQUIP:101,11) - FIGURE_SPLIT(EQUIP:101,10)))
ELSE
	LOCAL -= 1 * ((FIGURE_SPLIT(EQUIP:1,11) - FIGURE_SPLIT(EQUIP:1,10)))
ENDIF
RETURN LOCAL

@CLOTH_HOSEI_TAIRYOKU_101
LOCAL = 95
RETURN LOCAL
@CLOTH_HOSEI_KIRYOKU_101
LOCAL = 95
RETURN LOCAL

;----------------------------------------------------------
;固有カスタマイズ
@CLOTH_CUSTOMIZE_OPTION_101,ARG
#DIM CUSTOM, 5
#DIM CUSTOM_NUM = 5
#DIM ID
RESULT = 0
ID = (CFLAG:ARG:1 > 0) * 100 + 1
REPEAT CUSTOM_NUM
	CUSTOM:COUNT = FIGURE_SPLIT(EQUIP:ARG:ID, COUNT + 1)
REND

SIF CUSTOM:0 == 0
	PRINT [カラー：白]
SIF CUSTOM:0 == 1
