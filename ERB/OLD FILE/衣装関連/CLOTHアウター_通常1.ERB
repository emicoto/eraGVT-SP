SIF CUSTOM:0 == 2
	PRINT [カラー：ピンク]

PRINT [セーラー服][スカーフ]

SIF CUSTOM:1 == 2
	PRINT [下乳露出]
SIF CUSTOM:1 == 1 || CUSTOM:1 == 2
	PRINT [ヘソ出し]

SIF CUSTOM:2 == 0
	PRINT [半袖]
SIF CUSTOM:2 == 1
	PRINT [長袖]
SIF CUSTOM:2 == 2
	PRINT [ぶかぶか長袖]
SIF CUSTOM:2 == 3
	PRINT [袖なし]

SIF CUSTOM:3 == 0
	PRINT [ミニスカート]
SIF CUSTOM:3 == 1
	PRINT [スカート丈長い]
SIF CUSTOM:3 == 2
	PRINT [超ミニ]
SIF CUSTOM:3 == 3
	PRINT [ブルマ]

SIF CUSTOM:3 != 1 && CUSTOM:4 == 3
	PRINT [絶対領域]

SIF CUSTOM:4 == 0
	PRINT [ソックス]
SIF CUSTOM:4 == 1
	PRINT [ハイソックス]
SIF CUSTOM:4 == 2
	PRINT [ルーズソックス]
SIF CUSTOM:4 == 3
	PRINT [ニーハイ]
SIF CUSTOM:4 == 4
	PRINT [黒タイツ]
SIF CUSTOM:4 == 5
	PRINT [包帯]

PRINTL 
SIF CFLAG:ARG:1 > 0 && CFLAG:ARG:(900 + ID) == 1
	PRINTL 　変身時追加：[豪華な装飾][軽装甲]
SIF CFLAG:ARG:1 > 0 && CFLAG:ARG:(900 + ID) == 2
	PRINTL 　変身時追加：[サークレット][ロンググローブ][ロングブーツ]
SIF CFLAG:ARG:1 > 0 && CFLAG:ARG:(900 + ID) == 3
	PRINTL 　変身時追加：[軽装甲][肩部大袖][手甲]
SIF CFLAG:ARG:1 > 0 && CFLAG:ARG:(900 + ID) == 4
	PRINTL 　変身時追加：[機動兵装ウィング][腰部武装ポッド]
DRAWLINE
PRINTL  [0] カラーリング
PRINTL  [10]布の面積
PRINTL  [20]袖の長さ
PRINTL  [30]スカート丈
PRINTL  [40]ソックス
SIF CFLAG:ARG:1 > 0
	PRINTL  [50]追加装備
PRINTL 
PRINTL  [90]初期化
IF CFLAG:ARG:40 == CFLAG:ARG:41
	IF CFLAG:ARG:1 > 0
		SIF EQUIP:ARG:ID == EQUIP:ARG:(ID - 100)
			SETCOLOR 96,96,96
		PRINTFORML  [91]変身前%ITEMNAME:ID%のカスタマイズをコピー
		PRINTFORML  [92]変身前%ITEMNAME:ID%のカスタマイズにペースト
	ELSE
		SIF EQUIP:ARG:ID == EQUIP:ARG:(ID + 100)
			SETCOLOR 96,96,96
		PRINTFORML  [91]変身後%ITEMNAME:(ID+100)%のカスタマイズをコピー
		PRINTFORML  [92]変身後%ITEMNAME:(ID+100)%のカスタマイズにペースト
	ENDIF
	RESETCOLOR
ENDIF
PRINTL  [99]戻る
$INPUT_LOOP
INPUT
IF RESULT == 0
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL カラーリングを選択してください
	SIF CUSTOM:0 == 0
		SETCOLOR 120,250,0
	PRINTL  [0]白
	RESETCOLOR
	SIF CUSTOM:0 == 1
		SETCOLOR 120,250,0
	PRINTL  [1]黒
	RESETCOLOR
	SIF CUSTOM:0 == 2
		SETCOLOR 120,250,0
	PRINTL  [2]ピンク
	RESETCOLOR
	$INPUT_LOOP_0
	INPUT
	IF RESULT >= 0 && RESULT <= 2
		CUSTOM:0 = RESULT
	ELSE
		CLEARLINE 1
		GOTO INPUT_LOOP_0
	ENDIF
ELSEIF RESULT == 10
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 布の面積を選択してください
	SIF CUSTOM:1 == 0
		SETCOLOR 120,250,0
	PRINTL  [0]通常
	RESETCOLOR
	SIF CUSTOM:1 == 1
		SETCOLOR 120,250,0
	PRINTL  [1]少なめ　　　　　　　　　　　耐久-5  命中+1
	RESETCOLOR
	SIF CUSTOM:1 == 2
		SETCOLOR 120,250,0
	PRINTL  [2]かなり少ない　　　　　　　　耐久-10 命中+2
	RESETCOLOR
	$INPUT_LOOP_1
	INPUT
	IF RESULT >= 0 && RESULT <= 2
		CUSTOM:1 = RESULT
	ELSE
		CLEARLINE 1
		GOTO INPUT_LOOP_1
	ENDIF
ELSEIF RESULT == 20
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 袖の長さを選択してください
	SIF CUSTOM:2 == 0
		SETCOLOR 120,250,0
	PRINTL  [0]半袖
	RESETCOLOR
	SIF CUSTOM:2 == 1
		SETCOLOR 120,250,0
	PRINTL  [1]長袖　　　　　　　　　　　　耐久+5  命中-1
	RESETCOLOR
	SIF CUSTOM:2 == 2
		SETCOLOR 120,250,0
	PRINTL  [2]ぶかぶか　　　　　　　　　　耐久+5  命中-1
	RESETCOLOR
	$INPUT_LOOP_2
	INPUT
	IF RESULT >= 0 && RESULT <= 2
		CUSTOM:2 = RESULT
	ELSE
		CLEARLINE 1
		GOTO INPUT_LOOP_2
	ENDIF
ELSEIF RESULT == 30
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL スカート丈を選択してください
	SIF CUSTOM:3 == 0
		SETCOLOR 120,250,0
	PRINTL  [0]ミニスカート
	RESETCOLOR
	SIF CUSTOM:3 == 1
		SETCOLOR 120,250,0
	PRINTL  [1]ロングスカート　　　　　　　　　　　耐久+10 命中-2 下着保護+10
	RESETCOLOR
	SIF CUSTOM:3 == 2
		SETCOLOR 120,250,0
	PRINTL  [2]お尻が見えなそうな超ミニ　　　　　　耐久-10 命中+2 下着保護-10
	RESETCOLOR
	SIF CUSTOM:3 == 3
		SETCOLOR 120,250,0
	PRINTL  [3]スカートではなくブルマ　　　　　　　耐久-5  命中+1 下着保護-5
	RESETCOLOR
	$INPUT_LOOP_3
	INPUT
	IF RESULT >= 0 && RESULT <= 3
		CUSTOM:3 = RESULT
	ELSE
		CLEARLINE 1
		GOTO INPUT_LOOP_3
	ENDIF
ELSEIF RESULT == 40
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL ソックスを選択してください
	SIF CUSTOM:4 == 0
		SETCOLOR 120,250,0
	PRINTL  [0]ソックス
	RESETCOLOR
	SIF CUSTOM:4 == 1
		SETCOLOR 120,250,0
	PRINTL  [1]ハイソックス
	RESETCOLOR
	SIF CUSTOM:4 == 2
		SETCOLOR 120,250,0
	PRINTL  [2]ルーズソックス
	RESETCOLOR
	SIF CUSTOM:4 == 3
		SETCOLOR 120,250,0
	PRINTL  [3]ニーハイ
	RESETCOLOR
	SIF CUSTOM:4 == 4
		SETCOLOR 120,250,0
	PRINTL  [4]黒タイツ　　　　　　　　　　耐久+5  命中-1 下着保護+5
	RESETCOLOR
	SIF CUSTOM:4 == 5
		SETCOLOR 120,250,0
	PRINTL  [5]包帯
	RESETCOLOR
	$INPUT_LOOP_4
	INPUT
	IF RESULT >= 0 && RESULT <= 5
		CUSTOM:4 = RESULT
	ELSE
		CLEARLINE 1
		GOTO INPUT_LOOP_4
	ENDIF
ELSEIF RESULT == 50 && CFLAG:ARG:1 > 0
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 追加装備を選択してください
	SIF CFLAG:ARG:(900 + ID) == 0
		SETCOLOR 120,250,0
	PRINTL  [0]追加装備なし
	RESETCOLOR
	SIF CFLAG:ARG:(900 + ID) == 1
		SETCOLOR 120,250,0
	PRINTL  [1]オーソドックス　　　　　　（豪華な装飾＋軽装甲）
	RESETCOLOR
	SIF CFLAG:ARG:(900 + ID) == 2
		SETCOLOR 120,250,0
	PRINTL  [2]クラシック　　　　　　　　（サークレット＋ロンググローブ＋ハイブーツ）
	RESETCOLOR
	SIF CFLAG:ARG:(900 + ID) == 3
		SETCOLOR 120,250,0
	PRINTL  [3]サムライセーラー　　　　　（軽装甲＋肩部大袖＋手甲）
	RESETCOLOR
	SIF CFLAG:ARG:(900 + ID) == 4
		SETCOLOR 120,250,0
	PRINTL  [4]機動兵装: 遊撃仕様　　　　（機動兵装ウィング＋腰部武装ポッド）
	RESETCOLOR
	$INPUT_LOOP_5
	INPUT
	IF RESULT >= 0 && RESULT <= 4
		CFLAG:ARG:(900 + ID) = RESULT
	ELSE
		CLEARLINE 1
		GOTO INPUT_LOOP_5
	ENDIF
ELSEIF RESULT == 90
	EQUIP:ARG:ID = 0
	SIF CFLAG:ARG:1 > 0
		CFLAG:ARG:(900 + ID) = 0
	VARSET CUSTOM
ELSEIF RESULT == 91 && CFLAG:ARG:40 == CFLAG:ARG:41
	IF CFLAG:ARG:1 > 0
		REPEAT CUSTOM_NUM
			CUSTOM:COUNT = FIGURE_SPLIT(EQUIP:ARG:(ID - 100), COUNT + 1)
		REND
	ELSE
		REPEAT CUSTOM_NUM
			CUSTOM:COUNT = FIGURE_SPLIT(EQUIP:ARG:(ID + 100), COUNT + 1)
		REND
	ENDIF
ELSEIF RESULT == 92 && CFLAG:ARG:40 == CFLAG:ARG:41
ELSEIF RESULT == 99
	RETURN 99
ELSE
	CLEARLINE 1
	GOTO INPUT_LOOP
ENDIF
LOCAL = 0
REPEAT CUSTOM_NUM
	SIF COUNT == 0
		CONTINUE
	LOCAL += CUSTOM:COUNT * POWER(10, COUNT)
REND
LOCAL += CUSTOM:0
EQUIP:ARG:ID = LOCAL

;露出度合い
LOCAL = 0
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 2) == 1
	LOCAL -= 1
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 2) == 2
	LOCAL -= 2
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 4) == 2
	LOCAL -= 2
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 4) == 3
	LOCAL -= 1

SIF FIGURE_SPLIT(EQUIP:ARG:ID, 3) == 1 || FIGURE_SPLIT(EQUIP:ARG:ID, 3) == 2
	LOCAL += 1
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 4) == 1
	LOCAL += 2
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 5) == 4
	LOCAL += 1

;補正効果
;軽装
SIF LOCAL < 0
	EQUIP:ARG:ID -= LOCAL * POWER(10, 9)
;重装
SIF LOCAL > 0
	EQUIP:ARG:ID += LOCAL * POWER(10, 10)
;下着保護ダウン
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 4) == 2
	EQUIP:ARG:ID += 2 * POWER(10, 11)
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 4) == 3
	EQUIP:ARG:ID += 1 * POWER(10, 11)
;下着保護アップ
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 4) == 1
	EQUIP:ARG:ID += 2 * POWER(10, 12)
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 5) == 4
	EQUIP:ARG:ID += 1 * POWER(10, 12)

;ペースト処理
IF RESULT == 92 && CFLAG:ARG:40 == CFLAG:ARG:41
	IF CFLAG:ARG:1 > 0
		EQUIP:ARG:(ID - 100) = EQUIP:ARG:ID
	ELSE
		EQUIP:ARG:(ID + 100) = EQUIP:ARG:ID
	ENDIF
ENDIF

RETURN -1

