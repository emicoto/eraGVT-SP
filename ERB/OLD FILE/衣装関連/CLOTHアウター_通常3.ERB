	PRINTL  [0]追加装備なし
	RESETCOLOR
	SIF CFLAG:ARG:(900 + ID) == 1
		SETCOLOR 120,250,0
	PRINTL  [1]オーソドックス　　　　　　（豪華な装飾＋軽装甲）
	RESETCOLOR
	SIF CFLAG:ARG:(900 + ID) == 2
		SETCOLOR 120,250,0
	PRINTL  [2]部分甲冑　　　　　　　　　（ガントレット＋グリーブ）
	RESETCOLOR
	SIF CFLAG:ARG:(900 + ID) == 3
		SETCOLOR 120,250,0
	PRINTL  [3]機動兵装: 遊撃仕様　　　　（機動兵装ウィング＋腰部武装ポッド）
	RESETCOLOR
	SIF CFLAG:ARG:(900 + ID) == 4
		SETCOLOR 120,250,0
	PRINTL  [4]機動兵装: 突撃仕様　　　　（大型武装ナックル＋脚部スラスター）
	RESETCOLOR
	SIF CFLAG:ARG:(900 + ID) == 5
		SETCOLOR 120,250,0
	PRINTL  [5]エンジェル　　　　　　　　（天使の光輪＋天使の羽）
	RESETCOLOR
	SIF CFLAG:ARG:(900 + ID) == 6
		SETCOLOR 120,250,0
	PRINTL  [6]ストライクウィッチ　　　　（ケモみみ＋動物のしっぽ＋脚部装着式推進機）
	RESETCOLOR
	$INPUT_LOOP_5
	INPUT
	IF RESULT >= 0 && RESULT <= 6
		CFLAG:ARG:(900 + ID) = RESULT
	ELSE
		CLEARLINE 1
		GOTO INPUT_LOOP_5
	ENDIF
ELSEIF RESULT == 90
	EQUIP:ARG:ID = 0
	SIF CFLAG:ARG:1 > 0
		CFLAG:ARG:(900 + ID) = 0
	VARSET CUSTOM
ELSEIF RESULT == 91 && CFLAG:ARG:40 == CFLAG:ARG:41
	IF CFLAG:ARG:1 > 0
		REPEAT CUSTOM_NUM
			CUSTOM:COUNT = FIGURE_SPLIT(EQUIP:ARG:(ID - 100), COUNT + 1)
		REND
	ELSE
		REPEAT CUSTOM_NUM
			CUSTOM:COUNT = FIGURE_SPLIT(EQUIP:ARG:(ID + 100), COUNT + 1)
		REND
	ENDIF
ELSEIF RESULT == 92 && CFLAG:ARG:40 == CFLAG:ARG:41
ELSEIF RESULT == 99
	RETURN 99
ELSE
	CLEARLINE 1
	GOTO INPUT_LOOP
ENDIF
LOCAL = 0
REPEAT CUSTOM_NUM
	SIF COUNT == 0
		CONTINUE
	LOCAL += CUSTOM:COUNT * POWER(10, COUNT)
REND
LOCAL += CUSTOM:0
EQUIP:ARG:ID = LOCAL

;露出度合い
LOCAL = 0
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 3) == 1
	LOCAL += 1
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 3) == 2
	LOCAL += 1
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 3) == 3
	LOCAL += 1
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 3) == 4
	LOCAL += 1
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 4) == 1
	LOCAL += 1
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 4) == 2
	LOCAL += 1
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 5) == 4
	LOCAL += 1

;補正効果
;軽装
SIF LOCAL < 0
	EQUIP:ARG:ID -= LOCAL * POWER(10, 9)
;重装
SIF LOCAL > 0
	EQUIP:ARG:ID += LOCAL * POWER(10, 10)
;下着保護アップ
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 5) == 4
	EQUIP:ARG:ID += 1 * POWER(10, 12)

;ペースト処理
IF RESULT == 92 && CFLAG:ARG:40 == CFLAG:ARG:41
	IF CFLAG:ARG:1 > 0
		EQUIP:ARG:(ID - 100) = EQUIP:ARG:ID
	ELSE
		EQUIP:ARG:(ID + 100) = EQUIP:ARG:ID
	ENDIF
ENDIF

RETURN -1
