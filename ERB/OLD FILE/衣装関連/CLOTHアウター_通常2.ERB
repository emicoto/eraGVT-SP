	LOCAL -= 1 * ((FIGURE_SPLIT(EQUIP:145,11) - FIGURE_SPLIT(EQUIP:145,10)))
ELSE
	LOCAL -= 1 * ((FIGURE_SPLIT(EQUIP:45,11) - FIGURE_SPLIT(EQUIP:45,10)))
ENDIF
RETURN LOCAL

