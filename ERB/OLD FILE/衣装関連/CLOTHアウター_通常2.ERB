;----------------------------------------------------------
;固有カスタマイズ
@CLOTH_CUSTOMIZE_OPTION_145,ARG
#DIM CUSTOM, 5
#DIM CUSTOM_NUM = 5
#DIM ID
RESULT = 0
ID = (CFLAG:ARG:1 > 0) * 100 + 45
REPEAT CUSTOM_NUM
	CUSTOM:COUNT = FIGURE_SPLIT(EQUIP:ARG:ID, COUNT + 1)
REND

SIF CUSTOM:0 == 0
	PRINT [フード]
SIF CUSTOM:0 == 1
	PRINT [死神の仮面]

PRINT [漆黒の外套][ビキニトップ]

SIF CUSTOM:2 == 0
	PRINT [グローブ]
SIF CUSTOM:2 == 1
	PRINT [手甲]

PRINT [ヘソ出し]

SIF CUSTOM:3 == 0
	PRINT [ホットパンツ]
SIF CUSTOM:3 == 1
	PRINT [超ミニ]
SIF CUSTOM:3 == 2
	PRINT [ビキニボトム]

SIF CUSTOM:4 != 0
	PRINT [絶対領域]

SIF CUSTOM:4 == 0
	PRINT [素脚]
SIF CUSTOM:4 == 1
	PRINT [ニーハイ]
SIF CUSTOM:4 == 2
	PRINT [サイハイブーツ]
SIF CUSTOM:4 == 3
	PRINT [ガーターストッキング]

PRINTL 
SIF CFLAG:ARG:1 > 0 && CFLAG:ARG:(900 + ID) == 1
	PRINTL 　変身時追加：[豪華な装飾][軽装甲]
SIF CFLAG:ARG:1 > 0 && CFLAG:ARG:(900 + ID) == 2
	PRINTL 　変身時追加：[大型武装ナックル][脚部スラスター]
SIF CFLAG:ARG:1 > 0 && CFLAG:ARG:(900 + ID) == 3
	PRINTL 　変身時追加：[黒天使の光輪][黒天使の羽]
DRAWLINE
PRINTL  [0] 装飾
PRINTL  
PRINTL  [20]腕部
PRINTL  [30]下半身
PRINTL  [40]脚部
SIF CFLAG:ARG:1 > 0
	PRINTL  [50]追加装備
PRINTL 
PRINTL  [90]初期化
IF CFLAG:ARG:40 == CFLAG:ARG:41
	IF CFLAG:ARG:1 > 0
		SIF EQUIP:ARG:ID == EQUIP:ARG:(ID - 100)
			SETCOLOR 96,96,96
		PRINTFORML  [91]変身前%ITEMNAME:ID%のカスタマイズをコピー
		PRINTFORML  [92]変身前%ITEMNAME:ID%のカスタマイズにペースト
	ELSE
		SIF EQUIP:ARG:ID == EQUIP:ARG:(ID + 100)
			SETCOLOR 96,96,96
		PRINTFORML  [91]変身後%ITEMNAME:(ID+100)%のカスタマイズをコピー
		PRINTFORML  [92]変身後%ITEMNAME:(ID+100)%のカスタマイズにペースト
	ENDIF
	RESETCOLOR
ENDIF
PRINTL  [99]戻る
$INPUT_LOOP
INPUT
IF RESULT == 0
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 頭部の装備を選択してください
	SIF CUSTOM:0 == 0
		SETCOLOR 120,250,0
	PRINTL  [0]装飾なし
	RESETCOLOR
	SIF CUSTOM:0 == 1
		SETCOLOR 120,250,0
	PRINTL  [1]死神の仮面
	RESETCOLOR
	$INPUT_LOOP_0
	INPUT
	IF RESULT >= 0 && RESULT <= 1
		CUSTOM:0 = RESULT
	ELSE
		CLEARLINE 1
		GOTO INPUT_LOOP_0
	ENDIF
ELSEIF RESULT == 20
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 腕部の装備を選択してください
	SIF CUSTOM:2 == 0
		SETCOLOR 120,250,0
	PRINTL  [0]指抜きグローブ
	RESETCOLOR
	SIF CUSTOM:2 == 1
		SETCOLOR 120,250,0
	PRINTL  [1]ガントレット　　　　　　　　耐久+5  命中-1
	RESETCOLOR
	$INPUT_LOOP_2
	INPUT
	IF RESULT >= 0 && RESULT <= 1
		CUSTOM:2 = RESULT
	ELSE
		CLEARLINE 1
		GOTO INPUT_LOOP_2
	ENDIF
ELSEIF RESULT == 30
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 腰部の装備を選択してください
	SIF CUSTOM:3 == 0
		SETCOLOR 120,250,0
	PRINTL  [0]ホットパンツ
	RESETCOLOR
	SIF CUSTOM:3 == 1
		SETCOLOR 120,250,0
	PRINTL  [1]超ミニ　　　　　　　　　　 耐久-5  命中+1 下着保護 -5
	RESETCOLOR
	SIF CUSTOM:3 == 2
		SETCOLOR 120,250,0
	PRINTL  [2]ビキニボトム　　　　　　　 耐久-10 命中+2 下着保護 -10
	RESETCOLOR
	$INPUT_LOOP_3
	INPUT
	IF RESULT >= 0 && RESULT <= 2
		CUSTOM:3 = RESULT
	ELSE
		CLEARLINE 1
		GOTO INPUT_LOOP_3
	ENDIF
ELSEIF RESULT == 40
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL ソックスを選択してください
	SIF CUSTOM:4 == 0
		SETCOLOR 120,250,0
	PRINTL  [0]素脚
	RESETCOLOR
	SIF CUSTOM:4 == 1
		SETCOLOR 120,250,0
	PRINTL  [1]ニーハイ
	RESETCOLOR
	SIF CUSTOM:4 == 2
		SETCOLOR 120,250,0
	PRINTL  [2]サイハイブーツ
	RESETCOLOR
	SIF CUSTOM:4 == 3
		SETCOLOR 120,250,0
	PRINTL  [3]ガーターストッキング
	RESETCOLOR
	$INPUT_LOOP_4
	INPUT
	IF RESULT >= 0 && RESULT <= 3
		CUSTOM:4 = RESULT
	ELSE
		CLEARLINE 1
		GOTO INPUT_LOOP_4
	ENDIF
ELSEIF RESULT == 50 && CFLAG:ARG:1 > 0
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 追加装備を選択してください
	SIF CFLAG:ARG:(900 + ID) == 0
		SETCOLOR 120,250,0
	PRINTL  [0]追加装備なし
	RESETCOLOR
	SIF CFLAG:ARG:(900 + ID) == 1
		SETCOLOR 120,250,0
	PRINTL  [1]オーソドックス　　　　　　（豪華な装飾＋軽装甲）
	RESETCOLOR
	SIF CFLAG:ARG:(900 + ID) == 2
		SETCOLOR 120,250,0
	PRINTL  [2]機動兵装: 突撃仕様　　　　（大型武装ナックル＋脚部スラスター）
	RESETCOLOR
	SIF CFLAG:ARG:(900 + ID) == 3
		SETCOLOR 120,250,0
	PRINTL  [3]ブラックエンジェル　　　　（黒天使の光輪＋黒天使の羽）
	RESETCOLOR
	$INPUT_LOOP_5
	INPUT
	IF RESULT >= 0 && RESULT <= 3
		CFLAG:ARG:(900 + ID) = RESULT
	ELSE
		CLEARLINE 1
		GOTO INPUT_LOOP_5
	ENDIF
ELSEIF RESULT == 90
	EQUIP:ARG:ID = 0
	SIF CFLAG:ARG:1 > 0
		CFLAG:ARG:(900 + ID) = 0
	VARSET CUSTOM
ELSEIF RESULT == 91 && CFLAG:ARG:40 == CFLAG:ARG:41
	IF CFLAG:ARG:1 > 0
		REPEAT CUSTOM_NUM
			CUSTOM:COUNT = FIGURE_SPLIT(EQUIP:ARG:(ID - 100), COUNT + 1)
		REND
	ELSE
		REPEAT CUSTOM_NUM
			CUSTOM:COUNT = FIGURE_SPLIT(EQUIP:ARG:(ID + 100), COUNT + 1)
		REND
	ENDIF
ELSEIF RESULT == 92 && CFLAG:ARG:40 == CFLAG:ARG:41
ELSEIF RESULT == 99
	RETURN 99
ELSE
	CLEARLINE 1
	GOTO INPUT_LOOP
ENDIF
LOCAL = 0
REPEAT CUSTOM_NUM
	SIF COUNT == 0
		CONTINUE
	LOCAL += CUSTOM:COUNT * POWER(10, COUNT)
REND
LOCAL += CUSTOM:0
EQUIP:ARG:ID = LOCAL

;露出度合い
LOCAL = 0

SIF FIGURE_SPLIT(EQUIP:ARG:ID, 3) == 1
	LOCAL += 1

SIF FIGURE_SPLIT(EQUIP:ARG:ID, 4) == 1
	LOCAL -= 1
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 4) == 2
	LOCAL -= 2

;補正効果
;軽装
SIF LOCAL < 0
	EQUIP:ARG:ID -= LOCAL * POWER(10, 9)
;重装
SIF LOCAL > 0
	EQUIP:ARG:ID += LOCAL * POWER(10, 10)

;下着保護ダウン
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 4) == 1
	EQUIP:ARG:ID += 1 * POWER(10, 11)
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 4) == 2
	EQUIP:ARG:ID += 2 * POWER(10, 11)

;ペースト処理
IF RESULT == 92 && CFLAG:ARG:40 == CFLAG:ARG:41
	IF CFLAG:ARG:1 > 0
		EQUIP:ARG:(ID - 100) = EQUIP:ARG:ID
	ELSE
		EQUIP:ARG:(ID + 100) = EQUIP:ARG:ID
	ENDIF
ENDIF

RETURN -1
