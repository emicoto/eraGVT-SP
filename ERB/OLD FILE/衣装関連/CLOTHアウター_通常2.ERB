;----------------------------------------------------------
@CLOTH_DESCRIPTION_119
PRINTL  やんちゃな男の子向けの短パンＴシャツにジャケット。女の子が着るとおてんばな印象に。
PRINTL 【 補正 】補正なし
PRINTL 【耐久度】120

;----------------------------------------------------------
;衣装の耐久度
@CLOTH_HP_119
LOCAL = 120
IF CFLAG:1 > 0
	LOCAL += 5 * (FIGURE_SPLIT(EQUIP:119,11) - FIGURE_SPLIT(EQUIP:119,10))
ELSE
	LOCAL += 5 * (FIGURE_SPLIT(EQUIP:19,11) - FIGURE_SPLIT(EQUIP:19,10))
ENDIF
RETURN LOCAL

;----------------------------------------------------------
;補正
@CLOTH_DEF_119
LOCAL = DEFAULT_OUTER_DEF
IF CFLAG:1 > 0
	LOCAL -= 5 * (FIGURE_SPLIT(EQUIP:119,13) - FIGURE_SPLIT(EQUIP:119,12))
ELSE
	LOCAL -= 5 * (FIGURE_SPLIT(EQUIP:19,13) - FIGURE_SPLIT(EQUIP:19,12))
ENDIF
RETURN LOCAL

@CLOTH_HOSEI_HIT_119
LOCAL = 0
IF CFLAG:1 > 0
	LOCAL -= 1 * ((FIGURE_SPLIT(EQUIP:119,11) - FIGURE_SPLIT(EQUIP:119,10)))
ELSE
	LOCAL -= 1 * ((FIGURE_SPLIT(EQUIP:19,11) - FIGURE_SPLIT(EQUIP:19,10)))
ENDIF
RETURN LOCAL

@CLOTH_HOSEI_TAIRYOKU_119
LOCAL = 95
RETURN LOCAL
@CLOTH_HOSEI_KIRYOKU_119
