	INPUT
	IF RESULT >= 0 && RESULT <= 3
		CFLAG:ARG:(900 + ID) = RESULT
	ELSE
		CLEARLINE 1
		GOTO INPUT_LOOP_6
	ENDIF
ELSEIF RESULT == 90
	EQUIP:ARG:ID = 0
	SIF CFLAG:ARG:1 > 0
		CFLAG:ARG:(900 + ID) = 0
	VARSET CUSTOM
ELSEIF RESULT == 91 && CFLAG:ARG:40 == CFLAG:ARG:41
	IF CFLAG:ARG:1 > 0
		REPEAT CUSTOM_NUM
			CUSTOM:COUNT = FIGURE_SPLIT(EQUIP:ARG:(ID - 100), COUNT + 1)
		REND
	ELSE
		REPEAT CUSTOM_NUM
			CUSTOM:COUNT = FIGURE_SPLIT(EQUIP:ARG:(ID + 100), COUNT + 1)
		REND
	ENDIF
ELSEIF RESULT == 92 && CFLAG:ARG:40 == CFLAG:ARG:41
ELSEIF RESULT == 99
	RETURN 99
ELSE
	CLEARLINE 1
	GOTO INPUT_LOOP
ENDIF
LOCAL = 0
REPEAT CUSTOM_NUM
	SIF COUNT == 0
		CONTINUE
	LOCAL += CUSTOM:COUNT * POWER(10, COUNT)
REND
LOCAL += CUSTOM:0
EQUIP:ARG:ID = LOCAL

;露出度合い
LOCAL = 0
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 3) == 1
	LOCAL -= 1
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 3) == 2
	LOCAL -= 1
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 3) == 3
	LOCAL -= 2
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 3) == 5
	LOCAL -= 1
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 3) == 6
	LOCAL -= 2
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 3) == 7
	LOCAL -= 2
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 3) == 8
	LOCAL -= 3
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 5) == 4
	LOCAL -= 1

SIF FIGURE_SPLIT(EQUIP:ARG:ID, 3) == 4
	LOCAL += 1
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 4) == 1
	LOCAL += 1
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 5) == 1
	LOCAL += 2
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 5) == 2
	LOCAL += 2
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 6) == 3
	LOCAL += 1

;補正効果
;軽装
SIF LOCAL < 0
	EQUIP:ARG:ID -= LOCAL * POWER(10, 9)
;重装
SIF LOCAL > 0
	EQUIP:ARG:ID += LOCAL * POWER(10, 10)

;下着保護ダウン
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 5) == 4
	EQUIP:ARG:ID += 1 * POWER(10, 11)

;下着保護アップ
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 5) == 1
	EQUIP:ARG:ID += 2 * POWER(10, 12)
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 5) == 2
	EQUIP:ARG:ID += 2 * POWER(10, 12)
SIF FIGURE_SPLIT(EQUIP:ARG:ID, 6) == 3
	EQUIP:ARG:ID += 1 * POWER(10, 12)

;ペースト処理
IF RESULT == 92 && CFLAG:ARG:40 == CFLAG:ARG:41
	IF CFLAG:ARG:1 > 0
		EQUIP:ARG:(ID - 100) = EQUIP:ARG:ID
	ELSE
		EQUIP:ARG:(ID + 100) = EQUIP:ARG:ID
	ENDIF
ENDIF

RETURN -1

