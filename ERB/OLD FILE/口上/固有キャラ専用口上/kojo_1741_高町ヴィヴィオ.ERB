;=============== 戦闘スタイル変更条件 ================
@KOJO_1741_BATTLE_CHARA_BATTLE_STYLE_CHANGE
;雑魚戦(攻撃回数を上げるため)
IF FLAG:10 == 2
	SELECTCASE RAND:2
		CASE 0
		CDFLAG:TARGET:近距離:戦闘スタイル=2;装甲
		CDFLAG:TARGET:中距離:戦闘スタイル=2;装甲
		CDFLAG:TARGET:遠距離:戦闘スタイル=2;装甲
		CASE 2
		CDFLAG:TARGET:近距離:戦闘スタイル=8;設置
		CDFLAG:TARGET:中距離:戦闘スタイル=8;設置
		CDFLAG:TARGET:遠距離:戦闘スタイル=8;設置
		CASEELSE
		CDFLAG:TARGET:近距離:戦闘スタイル=7;知略
		CDFLAG:TARGET:中距離:戦闘スタイル=7;知略
		CDFLAG:TARGET:遠距離:戦闘スタイル=7;知略
			
	ENDSELECT
ELSE
	;敵のHPが1/30かつ3000以下
	IF (FLAG:13 < FLAG:12 /30) && (FLAG:13 <3000)
		CDFLAG:TARGET:近距離:戦闘スタイル=6;全力
		CDFLAG:TARGET:中距離:戦闘スタイル=6;全力
		CDFLAG:TARGET:遠距離:戦闘スタイル=6;全力


	;敵のHPが半分以上
	ELSEIF (FLAG:13 > FLAG:12 /2)
		CDFLAG:TARGET:近距離:戦闘スタイル=2;装甲
		CDFLAG:TARGET:中距離:戦闘スタイル=1;連続
		CDFLAG:TARGET:遠距離:戦闘スタイル=5;広範


	;敵のHPが半分以上で、通常攻撃
	ELSE
		CDFLAG:TARGET:近距離:戦闘スタイル=8;設置
		CDFLAG:TARGET:中距離:戦闘スタイル=0;通常
		CDFLAG:TARGET:遠距離:戦闘スタイル=0;通常
	ENDIF
ENDIF


