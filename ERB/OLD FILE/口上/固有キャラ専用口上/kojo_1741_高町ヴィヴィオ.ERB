@KOJO_1741_COLOR
SELECTCASE RAND:7
	CASE 6
		SETCOLOR 0xff0000
	CASE 5
		SETCOLOR 0xff6600
	CASE 4
		SETCOLOR 0xffff00
	CASE 3
		SETCOLOR 0x00ff00
	CASE 2
		SETCOLOR 0x66cccc
	CASE 1
		SETCOLOR 0x0000ff
	CASE 0
		SETCOLOR 0x6600ff
ENDSELECT


