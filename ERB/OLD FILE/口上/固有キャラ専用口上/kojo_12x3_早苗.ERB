VARSET LOCAL
;ショップ画面で選択されているときに一言しゃべります
;普段の様子や体調、今の心情など
SELECTCASE RAND:10
CASE 9
	PRINTFORML 「さて、今日も頑張るとしますか」
CASE 8
	FOR LOCAL,1,CHARANUM
		IF CALLNAME:LOCAL == "霊夢"
			IF !CFLAG:LOCAL:0
				LOCAL:1 = 1
			ELSE
				LOCAL:1 = 2
			ENDIF
		ENDIF
	NEXT
	IF LOCAL:1 == 1
		PRINTFORML 「霊夢さんもいることだし、ちょっと手を抜いても」
	ELSEIF LOCAL:1 == 2
		PRINTFORML 「霊夢さん、居なくなるなんてひどいです……」
	ELSE
		PRINTFORML 「はぁ、まさかこんなことになるなんて」
	ENDIF
CASE 7
	FOR LOCAL,1,CHARANUM
		IF CALLNAME:LOCAL == "魔理沙"
			IF !CFLAG:LOCAL:0
				LOCAL:1 = 1
			ELSE
				LOCAL:1 = 2
			ENDIF
		ENDIF
	NEXT
	IF LOCAL:1 == 1
		PRINTFORML 「魔理沙さんもいるし、気楽にいけますよね」
	ELSEIF LOCAL:1 == 2
		PRINTFORML 「魔理沙さんが、いなくなるなんて……」
	ELSE
		PRINTFORML 「はぁ、まさかこんなことになるなんて」
	ENDIF
CASE 6
	PRINTFORML 「スイーツ美味しい……。これだけは幻想郷にはないですしね」
CASE 5
	PRINTFORML 「奇跡が弱くなってる……、やっぱり外ではこうなりますよね」
CASE 4
	PRINTFORML 「……家、今どうなってるのかなぁ」
CASE 3
	PRINTFORML 「あんな触手がホントにいるなんて……」
CASE 2
	PRINTFORML 「……んー、早く帰りたいなぁ、神奈子様諏訪子様元気でいらっしゃると良いけど」
CASE 1
	PRINTFORML 「はぁしんどいなぁ」
CASE 0
	PRINTFORML 「いつまで触手退治しないといけないんでしょうかね」
ENDSELECT


