;C攻め弱
;LOCAL:0 快Ｃ
;LOCAL:1 快Ｖ
;LOCAL:2 快Ａ
;LOCAL:3 快Ｂ
;LOCAL:4 潤滑
;LOCAL:5 恭順
;LOCAL:6 習得
;LOCAL:7 欲情
;LOCAL:8 屈服
;LOCAL:9 恥情
;LOCAL:10 苦痛
;LOCAL:11 恐怖

;性コマンド分類
@SEX_TYPE_COM0
RETURN 通常

@SEX_COM0,ARG = 0,ARG:1 = 0
;EX_COM	追加責め実行部位（ビット）
;	1：Ｃ　2：Ｖ　4：Ａ　8：Ｂ
;追加責め
CALL SEX_COMEX_RANDOM, Ｃ, ARG:1
EX_COM = RESULT:0
SH_COM = RESULT:1

;取得パラメータ計算
;ベース値の明示的初期化
REPEAT 12
	LOCAL:COUNT = 0
REND

;油断度の上昇
TFLAG:3 += 25

;射精値 50
CALL TENTACLE_SYASEI_UP, 50

;触手サイズの設定
CALL SET_TENTACLE_SIZE, FLAG:10, FLAG:11, FLAG:110, FLAG:111, 0

IF FLAG:10 == 2
	CALLFORM MESSAGE_MOB_{ARG}_COM0, EX_COM, SH_COM
ELSE
	;地の文：洗脳/悪堕ちキャラが操作キャラにC攻め弱をした
	SIF FLAG:110 == 1
		CALL MESSAGE_OTHER_SEX_COM0
	;地の文：COM0
	CALL MESSAGE_SEX_COM0, EX_COM, SH_COM
ENDIF

;結界による触手本数の変動
CALL SET_TENTACLE_POOL

;服へのダメージ
CALL CLOTH_BATTLE_DAMAGE, 4

;ベース
;快C
IF ABL:Ｃ感覚 == 0
	LOCAL:0 = 500
ELSEIF ABL:Ｃ感覚 == 1
	LOCAL:0 = 1000
ELSEIF ABL:Ｃ感覚 == 2
	LOCAL:0 = 2000
ELSEIF ABL:Ｃ感覚 == 3
	LOCAL:0 = 4000
ELSEIF ABL:Ｃ感覚 == 4
	LOCAL:0 = 6000
ELSEIF ABL:Ｃ感覚 >= 5
	LOCAL:0 = 8000
ENDIF
;ペニス持ちには効果大
SIF TALENT:オトコ > 0 || TALENT:ふたなり > 0
	LOCAL:0 += 500

;追加責め実行
IF EX_COM
	CALL SEX_COMEX, Ｃ, 0, EX_COM | SH_COM
	REPEAT 12
		LOCAL:COUNT += RESULT:COUNT
	REND
ENDIF

;行動予約なし
TFLAG:17 = -1
;触手の行動を記録
TFLAG:20 = 0

;習得基本値
LOCAL:6 += 0

;欲情基本値
LOCAL:7 += 0

;屈服基本値
LOCAL:8 += 500

;恥情基本値
LOCAL:9 += 50

;苦痛基本値
LOCAL:10 += 0

;恐怖基本値
LOCAL:11 += 0

;体力消費基本値
LOCAL:12 += 50

;計算へ
CALL PALAM_CAL, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12
