;自慰強制

;性コマンド分類
@SEX_TYPE_COM1006
RETURN 通常

;本家eraのように自慰と強制自慰の区別はいまのところ無し
@SEX_SPCOM6

;取得パラメータ計算
;ベース値の明示的初期化
REPEAT 12
	LOCAL:COUNT = 0
REND

;油断度の上昇
TFLAG:3 += 50

;射精値 50
CALL TENTACLE_SYASEI_UP, 50

;触手サイズの設定
CALL SET_TENTACLE_SIZE, FLAG:10, FLAG:11, FLAG:110, FLAG:111, 1006

;地の文：洗脳/悪堕ちキャラが操作キャラに強制自慰をさせた
SIF FLAG:110 == 1
	CALL MESSAGE_OTHER_SEX_SPCOM6
	
;地の文：SPCOM6
CALL MESSAGE_SEX_SPCOM6

;結界による触手本数の変動
CALL SET_TENTACLE_POOL

;行動予約なし
TFLAG:17 = -1
;触手の行動を記録
TFLAG:20 = 1006

;ベースと計算は自慰の専用処理で行うのでここには記述しない
CALL SELF_KIND, TARGET, 0
