;稼ぎ・経験計算
@CALC_SEISAN, ARG:0, ARG:1
;体力の最大値
LOCAL:120 = MAXBASE:体力
;気力の最大値
LOCAL:130 = MAXBASE:気力
;性耐性の最大値
LOCAL:140 = MAXBASE:性耐性
;客の人数
LOCAL:121 = 0
;稼いだお金
LOCAL:131 = 0
;金銭獲得倍率
LOCAL:151 = 100
;FEAT効果による効率アップ
SIF TALENT:ラッキーチャーム > 0
	LOCAL:151 = 125

SELECTCASE ARG:0
	;アルバイト
	CASE 0
		SELECTCASE ARG:1
			;バイト成功
			CASE 0
				LOCAL:120 = LOCAL:120 * (20 + RAND:7) / 100
				LOCAL:130 = LOCAL:130 * (10 + RAND:4) / 100
				BASE:体力 = LIMIT(BASE:体力 - SYOUHI_KEIGEN(TARGET,LOCAL:120,体力), 0 , BASE:体力)
				BASE:気力 = LIMIT(BASE:気力 - SYOUHI_KEIGEN(TARGET,LOCAL:130,気力), 0 , BASE:気力)
				LOCAL:121 = MAXBASE:体力 - BASE:体力
				LOCAL:131 = LOCAL:151 * LOCAL:121 / 1000 + RAND:10 + 200 + ABL:レベル * 10
				MONEY += LOCAL:131
			;ヒーローショー失敗
			CASE 1
				LOCAL:120 = LOCAL:120 * (40 + RAND:7) / 100
