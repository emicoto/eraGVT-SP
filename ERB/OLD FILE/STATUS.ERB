LOCAL = 0
REPEAT 99
	IF ABLNAME:COUNT != ""
		SIF COUNT != 30 && COUNT != 31 && COUNT != 32 && COUNT != 33 && COUNT != 50
			PRINTFORM %ABLNAME:COUNT,8%：LV.{ABL:ARG:COUNT,2,LEFT}  
		SIF COUNT == 3 || COUNT == 13 || COUNT == 21
			PRINTL 
	ENDIF
REND

