;ステータス表示のベース（4ページ目）
@SHOW_STATUS_CHARA_SELECT4, ARG
#DIM LCOUNT
#DIM PAPA
#DIM MAMA
$REPRINT
CALL LB
DRAWLINE
PRINTL 　　　　　　　　　　　　　　　　　キャラクター相関　　　　　　　　　　　　　　　PAGE(4/5)
DRAWLINE
PRINTFORML 氏名　：%NAME:(ARG:0)%
PRINTFORML 呼び名：%CALLNAME:(ARG:0)%
PRINTFORML 一人称：%SELF_CALL(0, 0, ARG:0)%
PRINTFORML レベル：Lv.{ABL:(ARG:0):レベル} （{JUEL:(ARG:0):経験値}％）
LOCALS = \@TALENT:(ARG:0):オトコ > 0 ? 男 # 女\@
PRINTFORML 性別　：%LOCALS%
CALL SHORTLINE
LCOUNT = 0
FOR LOCAL, 0, CHARANUM
	SIF LOCAL == MASTER
		CONTINUE
	SIF LOCAL == ARG:0
		CONTINUE
	IF RELATION:(ARG:0):LOCAL
		CALL GET_RELATION, ARG:0, LOCAL
		PRINTFORML 　({LOCAL, 2,RIGHT})　%NAME:LOCAL%　→　%RESULTS%
		LCOUNT ++
	ENDIF
NEXT
IF LCOUNT == 0
	PRINTL 　　　相関関係のあるキャラ：なし
	LCOUNT ++
ENDIF
IF CFLAG:(ARG:0):120 > 0 || CFLAG:(ARG:0):121 > 0 || CFLAG:(ARG:0):122 > 0
	PRINTL 
	PRINTL 　　　コネクション：
	LCOUNT += 2
ENDIF
;コネクション関係
IF CFLAG:(ARG:0):120 == 1
	PRINTL 　　　　　　噂好きな友人
	LCOUNT ++
ENDIF
IF CFLAG:(ARG:0):121 == 1
	PRINTL 　　　　　　警察関係者
	LCOUNT ++
ELSEIF CFLAG:(ARG:0):121 == 2
	PRINTL 　　　　　　警察上層部
	LCOUNT ++
ENDIF
IF CFLAG:(ARG:0):122 > 0
	PRINT 　　　　　　情報屋(
	IF GETBIT(CFLAG:(ARG:0):122, 2) && GETBIT(CFLAG:(ARG:0):122, 3)
		PRINT 無法者
	ELSEIF GETBIT(CFLAG:(ARG:0):122, 2) && GETBIT(CFLAG:(ARG:0):122, 4)
		PRINT お調子者な男
	ELSEIF GETBIT(CFLAG:(ARG:0):122, 3) && GETBIT(CFLAG:(ARG:0):122, 4)
		PRINT 老紳士
	ELSEIF GETBIT(CFLAG:(ARG:0):122, 2)
		PRINT 下品な男
	ELSEIF GETBIT(CFLAG:(ARG:0):122, 3)
		PRINT 上品な男
	ELSEIF GETBIT(CFLAG:(ARG:0):122, 4)
		PRINT 探偵風の男
	ELSE
		PRINT 特徴の無い男
	ENDIF
	PRINTL )
	LCOUNT ++
ENDIF
IF CFLAG:(ARG:0):123 == 1
	PRINTL 　　　　　　裕福な実家
	LCOUNT ++
ENDIF

FOR LOCAL, 0, 20 - LCOUNT
	PRINTL 
NEXT
IF RELATION:(ARG:0):(ARG:0) > 0
	CALL SHORTLINE
	PRINT 　血統　(
	SIF GETBIT(RELATION:(ARG:0):(ARG:0), 血統不明)
		PRINT 　？？？
	SIF GETBIT(RELATION:(ARG:0):(ARG:0), Ｃ触手)
		PRINT 　Ｃ触手
	SIF GETBIT(RELATION:(ARG:0):(ARG:0), Ｖ触手)
		PRINT 　Ｖ触手
	SIF GETBIT(RELATION:(ARG:0):(ARG:0), Ａ触手)
		PRINT 　Ａ触手
	SIF GETBIT(RELATION:(ARG:0):(ARG:0), Ｂ触手)
		PRINT 　Ｂ触手
	SIF GETBIT(RELATION:(ARG:0):(ARG:0), Ｓ触手)
		PRINT 　Ｓ触手
	SIF GETBIT(RELATION:(ARG:0):(ARG:0), Ｐ触手)
		PRINT 　Ｐ触手
	SIF GETBIT(RELATION:(ARG:0):(ARG:0), Ｈ触手)
		PRINT 　Ｈ触手
	PRINTL 　)
ELSE
	PRINTL 
ENDIF
IF CFLAG:(ARG:0):7 || CFLAG:(ARG:0):9
	CALL SHORTLINE
ELSE
	PRINTL 
ENDIF
PAPA = 0
MAMA = 0
IF CFLAG:(ARG:0):9 <= -100
	FOR LOCAL, 0, CHARANUM
		SIF LOCAL == MASTER
			CONTINUE
		SIF LOCAL == ARG:0
			CONTINUE
		SIF CFLAG:LOCAL:240 == CFLAG:(ARG:0):9 * -1 - 100
			PAPA = LOCAL
	NEXT
ENDIF
IF CFLAG:(ARG:0):7 > 0
	FOR LOCAL, 0, CHARANUM
		SIF LOCAL == MASTER
			CONTINUE
		SIF LOCAL == ARG:0
			CONTINUE
		SIF CFLAG:LOCAL:240 == CFLAG:(ARG:0):7
			MAMA = LOCAL
	NEXT
ENDIF
IF PAPA > 0
	PRINTFORM 　実父　:　%NAME:PAPA% 
	SIF CFLAG:PAPA:34 > 0
		PRINTFORM ({BASE:PAPA:実年齢})
	IF GETBIT(RELATION:PAPA:MAMA,親子) || GETBIT(RELATION:PAPA:MAMA,兄弟姉妹) || GETBIT(RELATION:PAPA:MAMA,祖父祖母孫) || GETBIT(RELATION:PAPA:MAMA,おじおば) || GETBIT(RELATION:PAPA:MAMA,甥姪)
		PRINT 　　(
		IF GETBIT(RELATION:PAPA:MAMA,親子)
			IF (BASE:PAPA:実年齢 == BASE:MAMA:実年齢 && CFLAG:PAPA:240 < CFLAG:MAMA:240) || BASE:PAPA:実年齢 > BASE:MAMA:実年齢
				PRINT 娘
			ELSEIF (BASE:PAPA:実年齢 == BASE:MAMA:実年齢 && CFLAG:PAPA:240 > CFLAG:MAMA:240) || BASE:PAPA:実年齢 < BASE:MAMA:実年齢
				PRINT 母
			ENDIF
			SIF GETBIT(RELATION:PAPA:MAMA,兄弟姉妹) || GETBIT(RELATION:PAPA:MAMA,祖父祖母孫) || GETBIT(RELATION:PAPA:MAMA,おじおば) || GETBIT(RELATION:PAPA:MAMA,甥姪)
				PRINT /
		ENDIF
		IF GETBIT(RELATION:PAPA:MAMA,兄弟姉妹)
			IF (BASE:PAPA:実年齢 == BASE:MAMA:実年齢 && CFLAG:PAPA:240 < CFLAG:MAMA:240) || BASE:PAPA:実年齢 > BASE:MAMA:実年齢
				PRINT 妹
			ELSEIF (BASE:PAPA:実年齢 == BASE:MAMA:実年齢 && CFLAG:PAPA:240 > CFLAG:MAMA:240) || BASE:PAPA:実年齢 < BASE:MAMA:実年齢
				PRINT 姉
			ENDIF
			SIF GETBIT(RELATION:PAPA:MAMA,祖父祖母孫) || GETBIT(RELATION:PAPA:MAMA,おじおば) || GETBIT(RELATION:PAPA:MAMA,甥姪)
				PRINT /
		ENDIF
		IF GETBIT(RELATION:PAPA:MAMA,祖父祖母孫)
			IF (BASE:PAPA:実年齢 == BASE:MAMA:実年齢 && CFLAG:PAPA:240 < CFLAG:MAMA:240) || BASE:PAPA:実年齢 > BASE:MAMA:実年齢
				PRINT 孫
			ELSEIF (BASE:PAPA:実年齢 == BASE:MAMA:実年齢 && CFLAG:PAPA:240 > CFLAG:MAMA:240) || BASE:PAPA:実年齢 < BASE:MAMA:実年齢
				PRINT 祖母
			ENDIF
			SIF GETBIT(RELATION:PAPA:MAMA,おじおば) || GETBIT(RELATION:PAPA:MAMA,甥姪)
				PRINT /
		ENDIF
		IF GETBIT(RELATION:PAPA:MAMA,おじおば)
				PRINT おば
		ELSEIF GETBIT(RELATION:PAPA:MAMA,甥姪)
				PRINT 姪
		ENDIF
		PRINT との近親相姦)
	ENDIF
ELSEIF CFLAG:(ARG:0):9 > 0
	IF CFLAG:(ARG:0):9 < 100
			TRYCALLFORM TENTACLE_BOSS_{CFLAG:(ARG:0):9}_GETNAME
	ELSEIF CFLAG:(ARG:0):9 < 200
			TRYCALLFORM TENTACLE_LASTBOSS_{CFLAG:(ARG:0):9 - 100}_GETNAME
	ELSEIF CFLAG:(ARG:0):9 == 200
			RESULTS = 雑魚触手
	ELSE
			TRYCALLFORM TENTACLE_MOB_{CFLAG:(ARG:0):9 - 200}_GETNAME
	ENDIF
	PRINTFORM 　実父　:　%RESULTS%
ELSEIF CFLAG:(ARG:0):9 == -100
	PRINTFORM 　実父　:　誰とも知れない相手
ENDIF
PRINTL 
IF MAMA > 0
	PRINTFORM 　実母　:　%NAME:MAMA% 
	SIF CFLAG:MAMA:34 > 0
		PRINTFORM ({BASE:MAMA:実年齢})
	IF GETBIT(RELATION:MAMA:PAPA,親子) || GETBIT(RELATION:MAMA:PAPA,兄弟姉妹) || GETBIT(RELATION:MAMA:PAPA,祖父祖母孫) || GETBIT(RELATION:MAMA:PAPA,おじおば) || GETBIT(RELATION:MAMA:PAPA,甥姪)
