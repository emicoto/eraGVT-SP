IF RESULT == 999
	RETURN
ELSEIF RESULT == 0 && CFLAG:ARG:0 == 0 && FLAG:700 == 0
	CALL WEAPON_CUSTOMIZE, ARG
	GOTO REPRINT
ELSEIF RESULT == 1
	CALL SHOW_STATUS_CHARA_SELECT3, ARG
