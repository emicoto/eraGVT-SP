	SIF COUNT == 3
		PRINTL 
	PRINTFORM %MARKNAME:COUNT%：LV.{MARK:ARG:COUNT}   
REND

;--------------------------------------------------
;経験表示
@SHOW_STATUS_EXP, ARG
PRINTL ◆経験
LOCAL = 0
REPEAT 99
	;陥落経験は非表示
	SIF COUNT == 12
		CONTINUE
	;スカトロ描写offなら放尿経験は非表示
	IF COUNT == 55 && FILTER_CHECK_F(15) == 1
		PRINTL 
		CONTINUE
	ENDIF
	IF TALENT:ARG:変身能力 == -1
		SIF COUNT == 5 || COUNT == 6 || COUNT == 7
			CONTINUE
	ELSE
		SIF COUNT == 8
			CONTINUE
	ENDIF
	IF EXPNAME:COUNT != ""
		PRINTFORM %EXPNAME:COUNT,14%：{EXP:ARG:COUNT,8,LEFT}
		SIF COUNT == 2 || COUNT == 7 || COUNT == 13 || COUNT == 21 || COUNT == 24 || COUNT == 32 || COUNT == 42 || COUNT == 52 || COUNT == 55
			PRINTL 
		SIF COUNT == 8 && TALENT:ARG:変身能力 == -1
			PRINTL 
	ENDIF
REND

