;--------------------------------------------------
;専用ルーチン（戦闘）
@TENTACLE_BOSS_7_ATTACK_ROUTINE
LOCAL:0 = RAND:100
IF LOCAL:0 < 20
	RETURN 3
ELSE 
	RETURN 0
ENDIF

;--------------------------------------------------
;専用ルーチン（性行為）
@TENTACLE_BOSS_7_SEX_ROUTINE
;挿入系コマンドが少ないため、衣装の破壊の優先度アップ
SIF RAND:100 < 25
	RETURN 14

LOCAL = RAND:100
;恍惚だと催眠姦を仕掛ける確率UP
IF (TCVAR:12 & 恍惚) && RAND:100 < 50
	RETURN 1014
ELSEIF LOCAL < 5
	RETURN 0
ELSEIF LOCAL < 15
	RETURN 1006
ELSEIF LOCAL < 30
	RETURN 1007
ELSEIF LOCAL < 40
	RETURN 6
ELSEIF LOCAL < 43
	RETURN 2
ELSEIF LOCAL < 50
	RETURN 3
ELSEIF LOCAL < 60
	RETURN 11
ELSEIF LOCAL < 70
	RETURN 12
ELSEIF LOCAL < 75
	RETURN 13
ELSEIF LOCAL < 85
	RETURN 1014
ELSE
	RETURN -1
ENDIF

;--------------------------------------------------
;性コマンドレファレンス
;ARG=1　素股焦らし成功時の行動指定
;ARG=2　素股焦らし失敗時の行動指定
;ARG=3　フェラチオ誘発時の行動指定
@TENTACLE_BOSS_7_REACTION_REF, ARG = 0
IF ARG == 1
	LOCAL = RAND:6
	IF LOCAL < 2
		RETURN 3
	ELSEIF LOCAL < 4
		RETURN 5
	ELSEIF LOCAL < 5
		RETURN 1001
	ELSE
		RETURN 1002
	ENDIF
ELSEIF ARG == 2
	RETURN 15
ELSEIF ARG == 3
	RETURN 11
ENDIF
RETURN -1


;--------------------------------------------------
;専用ルーチン（幽閉）
@TENTACLE_BOSS_7_PRISON_ROUTINE
LOCAL = RAND:100
IF LOCAL < 30
	CALL PRISON_COMABLE, 6
	RETURN 1
ELSEIF LOCAL < 35
	CALL PRISON_COMABLE, 102
	RETURN 1
ELSEIF LOCAL < 40
	CALL PRISON_COMABLE, 201
	RETURN 1
ELSE
	RETURN 0
ENDIF

