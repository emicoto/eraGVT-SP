		LOCAL:1 += 1
NEXT
FOR LOCAL:3,0,LOCAL:1
	LOCAL = LOCAL:3 + 10
	SIF TALENT:ARG:LOCAL == 1
			RETURNF LOCAL
NEXT
RETURNF 0

