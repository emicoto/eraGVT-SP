;---------------------------------------------------------------------
;特定の素質を持っていると仲間に夜這いを掛ける
@YOBAI
#DIM CCOUNT
;昼間は判定しない
SIF TIME == 0
	RETURN
;キャラが一人しかいない場合は判定しない
SIF CHARANUM_ACTIVE() < 2
	RETURN
;夜這い対象にできるキャラが居ない場合は判定しない
LOCAL = 0
FOR CCOUNT, 0, CHARANUM
	SIF CCOUNT == MASTER
		CONTINUE
	;パーティに参加していないキャラは除外
	SIF CFLAG:CCOUNT:999 == 0
		CONTINUE
	;ここまで抜けたら人数にカウント
	LOCAL ++
NEXT
SIF LOCAL == 0
	RETURN
;ここまで抜けてくれば判定を開始
VARSET LOCAL
SETCOLOR 96,96,96
SIF FLAG:999 == 1
	PRINTL ------ 夜這い判定 ------
RESETCOLOR
;ランダム抽選用に候補をクリア
CALL CLEARRANDCHOOSE
FOR CCOUNT, 0, CHARANUM
	SIF CCOUNT == MASTER
		CONTINUE
	SIF CFLAG:CCOUNT:999 == 0
		CONTINUE

	TARGET = CCOUNT
	LOCAL = 0

	;淫核、淫壺、淫尻、淫乳、ふたなり、オトコの場合に発生判定を行う
	IF (TALENT:淫核 * 3 + ABL:Ｃ感覚 >= 3) || (TALENT:淫壷 * 3 + ABL:Ｖ感覚 >= 3) || (TALENT:淫尻 * 3 + ABL:Ａ感覚 >= 3) || (TALENT:淫乳 * 3 + ABL:Ｂ感覚 >= 3) || ((TALENT:ふたなり > 0 || TALENT:変身時ふたなり > 0 || TALENT:オトコ > 0) && ABL:射精中毒 > 0)
		;生存していないキャラは判定しない
		SIF CFLAG:0 != 0
			CONTINUE
		;確率判定
		LOCAL:1 = (TALENT:淫乱 > 0) * 2 + (TALENT:淫核 * 3 + ABL:Ｃ感覚 >= 3) + (TALENT:淫壷 * 3 + ABL:Ｖ感覚 >= 3) + (TALENT:淫尻 * 3 + ABL:Ａ感覚 >= 3) + (TALENT:淫乳 * 3 + ABL:Ｂ感覚 >= 3) + ((TALENT:ふたなり > 0 || TALENT:変身時ふたなり > 0  || TALENT:オトコ > 0) && ABL:射精中毒 > 0)
		LOCAL:2 = MAX(ABL:欲望 + ABL:自慰中毒 + ABL:触手中毒 , 0)
		SIF LOCAL:1
			LOCAL = (LOCAL:1 * 2 + 22) * (125 + LOCAL:2 * 10)
		SIF TALENT:交際相手 >= 1 && TALENT:交際相手 < 5
			TIMES LOCAL, 0.5
		SIF TALENT:清純派
			TIMES LOCAL, 0.5
		SIF TALENT:人間不信 > 0
			TIMES LOCAL, 0.25
		IF FLAG:999 == 1
			SETCOLOR 96,96,96
			PRINTFORML 実行判定　%CALLNAME:TARGET%　確率{LOCAL}
			RESETCOLOR
		ENDIF
		SIF RAND:10000 < LOCAL
			CALL ADDRANDCHOOSE,TARGET
	ELSE
		CONTINUE
	ENDIF
NEXT
$REROLL
;候補がある場合
IF CHOICECOUNT_F()
	TARGET = RANDCHOOSE_F()
	CALL YOBAI_EVENT
	IF RESULT == - 999
		CALL CLEARSPECIFICCHOOSE(TARGET)
		SETCOLOR 96,96,96
		SIF FLAG:999 == 1
			PRINTFORML %CALLNAME:TARGET% = -999 / REROLL
		RESETCOLOR
		GOTO REROLL
	ENDIF
	;実績
	CALL GET_STATE_EXPUP,TARGET
	;全キャラの変身を解除
	FOR LOCAL, 0, CHARANUM
		SIF LOCAL == MASTER
			CONTINUE
		SIF CFLAG:LOCAL:1 > 0 && CFLAG:LOCAL:0 == 0
			CALL TRANSFORM, 0, LOCAL
	NEXT
ELSE
	SETCOLOR 96,96,96
	SIF FLAG:999 == 1
		PRINTFORML 夜這い実行なし
	RESETCOLOR
ENDIF

;---------------------------------------------------------------------
;夜這いイベントの制御
@YOBAI_EVENT
#DIM LCOUNT
#DIM CCOUNT
#DIM SELECT,12
SIF CHARANUM_ACTIVE() == 0
	RETURN
;初期化
VARSET LOCAL
VARSET GFLAG
;現段階で対象者が居るかチェック
;双方が変身しなかった場合
SELECT = 0
;対象だけが変身した場合
SELECT:1 = 0
;実行者だけが変身した場合
SELECT:10 = 0
;双方が変身した場合
SELECT:11 = 0
FOR CCOUNT, 0, CHARANUM
	SIF CCOUNT == MASTER
		CONTINUE
	;実行者は除外
	SIF CCOUNT == TARGET
		CONTINUE
	;パーティに参加していないキャラは除外
	SIF CFLAG:CCOUNT:999 == 0
		CONTINUE

	;双方が変身しなかった場合
	;双方がオトコでなければカウント
	SIF CHARATALENT_F(TARGET,0,"オトコ") == 0 || CHARATALENT_F(CCOUNT,0,"オトコ") == 0
		SELECT:0 ++

	;相手だけが変身した場合
	;双方がオトコでなければカウント
	SIF CHARATALENT_F(TARGET,0,"オトコ") == 0 || CHARATALENT_F(CCOUNT,1,"オトコ") == 0
		SELECT:1 ++

	;実行者だけが変身した場合
	;双方がオトコでなければカウント
	SIF CHARATALENT_F(TARGET,1,"オトコ") == 0 || CHARATALENT_F(CCOUNT,0,"オトコ") == 0
		SELECT:10 ++

	;双方が変身した場合
	;双方がオトコでなければカウント
	SIF CHARATALENT_F(TARGET,1,"オトコ") == 0 || CHARATALENT_F(CCOUNT,1,"オトコ") == 0
		SELECT:11 ++

	;夜這い条件を満たしていない場合
	SETCOLOR 96,96,96
	IF CHARATALENT_F(TARGET,0,"ふたなり") > 0 && (TALENT:淫核 * 3 + ABL:Ｃ感覚 < 3) && (TALENT:淫壷 * 3 + ABL:Ｖ感覚 < 3) && (TALENT:淫尻 * 3 + ABL:Ａ感覚 < 3) && (TALENT:淫乳 * 3 + ABL:Ｃ感覚 < 3) && ABL:Ｃ感覚 + ABL:射精中毒 == 0
		SELECT:0 = 0
		SELECT:1 = 0
	ELSEIF CHARATALENT_F(TARGET,0,"オトコ") == 0 && (TALENT:淫核 * 3 + ABL:Ｃ感覚 < 3) && (TALENT:淫壷 * 3 + ABL:Ｖ感覚 < 3) && (TALENT:淫尻 * 3 + ABL:Ａ感覚 < 3) && (TALENT:淫乳 * 3 + ABL:Ｃ感覚 < 3)
		SELECT:0 = 0
		SELECT:1 = 0
	ELSEIF CHARATALENT_F(TARGET,0,"オトコ") > 0 && ABL:Ｃ感覚 + ABL:射精中毒 == 0
		SELECT:0 = 0
		SELECT:1 = 0
	ENDIF

	IF CHARATALENT_F(TARGET,1,"ふたなり") > 0 && (TALENT:淫核 * 3 + ABL:Ｃ感覚 < 3) && (TALENT:淫壷 * 3 + ABL:Ｖ感覚 < 3) && (TALENT:淫尻 * 3 + ABL:Ａ感覚 < 3) && (TALENT:淫乳 * 3 + ABL:Ｃ感覚 < 3) && ABL:Ｃ感覚 + ABL:射精中毒 == 0
		SELECT:10 = 0
		SELECT:11 = 0
	ELSEIF CHARATALENT_F(TARGET,1,"オトコ") == 0 && (TALENT:淫核 * 3 + ABL:Ｃ感覚 < 3) && (TALENT:淫壷 * 3 + ABL:Ｖ感覚 < 3) && (TALENT:淫尻 * 3 + ABL:Ａ感覚 < 3) && (TALENT:淫乳 * 3 + ABL:Ｃ感覚 < 3)
		SELECT:10 = 0
		SELECT:11 = 0
	ELSEIF CHARATALENT_F(TARGET,1,"オトコ") > 0 && ABL:Ｃ感覚 + ABL:射精中毒 == 0
		SELECT:10 = 0
		SELECT:11 = 0
	ENDIF

	IF CHARATALENT_F(TARGET,0,"オトコ") == 0 && CHARATALENT_F(TARGET,1,"オトコ") > 0 && TALENT:妊娠 > 0
		SELECT:10 = 0
		SELECT:11 = 0
	ENDIF

	IF CHARATALENT_F(CCOUNT,0,"オトコ") == 0 && CHARATALENT_F(CCOUNT,1,"オトコ") > 0 && TALENT:CCOUNT:妊娠 > 0
		SELECT:1 = 0
		SELECT:11 = 0
	ENDIF

	;女体受容なし
	IF CHARATALENT_F(TARGET,0,"オトコ") > 0 && TALENT:変身時ＴＳ > 0 && TALENT:女体受容 == 0
		SELECT:10 = 0
		SELECT:11 = 0
	ENDIF

	;性別嗜好による選別
	IF TALENT:CCOUNT:オトコ > 0 && TALENT:男性苦手 > 0 && TALENT:両刀 == 0 && RAND:4 == 0
		SELECT:0 = 0
		SELECT:1 = 0
		SELECT:10 = 0
		SELECT:11 = 0
		SIF FLAG:999 == 1
			PRINTL 男性苦手
	ENDIF
	IF TALENT:CCOUNT:オトコ <= 0 && TALENT:女性苦手 > 0 && TALENT:両刀 == 0 && RAND:4 == 0
		SELECT:0 = 0
		SELECT:1 = 0
		SELECT:10 = 0
		SELECT:11 = 0
		SIF FLAG:999 == 1
			PRINTL 女性苦手
	ENDIF
	RESETCOLOR
NEXT
