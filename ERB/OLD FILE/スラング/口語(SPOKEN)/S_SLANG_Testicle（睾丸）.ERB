;Testicle(睾丸)の淫語
@S_SLANG_Testicle(ARG)
#DIMS SLANGWORD_Testicle
#FUNCTIONS

;初期化
SLANGWORD_Testicle=

;部位呼称
IF TALENT:淫乱 > 0
	SELECTCASE RAND:3
		CASE 2
			SLANGWORD_Testicle += "精液製造機"
		CASE 1
			SLANGWORD_Testicle += "精液バンク"
		CASEELSE
			SLANGWORD_Testicle += "ザーメンタンク"
	ENDSELECT
ELSE
	SELECTCASE RAND:3
		CASE 2
			SLANGWORD_Testicle += "玉袋"
		CASE 1
			SLANGWORD_Testicle += "タマタマ"
		CASEELSE
			SLANGWORD_Testicle += "金玉"
	ENDSELECT
ENDIF


;合成して出来上がった淫語を返す
RETURNF SLANGWORD_Testicle





