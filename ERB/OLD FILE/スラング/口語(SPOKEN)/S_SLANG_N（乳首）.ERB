;**********************************************************
;Nipple(ニップル)の淫語
;**********************************************************
@S_SLANG_N(ARG)
#DIMS S_SLANGWORD_N
#FUNCTIONS

;初期化
S_SLANGWORD_N=
IF TALENT:淫乳
	S_SLANGWORD_N += "乳芽"
ELSE
	SELECTCASE RAND:5
		CASE 4
			S_SLANGWORD_N += "乳頭"
		CASE 3
			S_SLANGWORD_N += "胸の先端"
		CASE 2
			S_SLANGWORD_N += "乳先"
		CASE 1
			S_SLANGWORD_N += "乳首"
		CASEELSE
			S_SLANGWORD_N += "ニップル"
	ENDSELECT
ENDIF

RETURNF S_SLANGWORD_N


