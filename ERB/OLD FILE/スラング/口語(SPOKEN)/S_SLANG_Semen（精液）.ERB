;**********************************************************
;Spoken_Semen(精液)の口述淫語
;**********************************************************
@S_SLANG_Semen(ARG)
#DIMS S_SLANGWORD_Semen
#FUNCTIONS

;初期化
S_SLANGWORD_Semen=


;部位呼称
IF TALENT:清純派
	SELECTCASE RAND:3
		CASE 2
			S_SLANGWORD_Semen += "白いオシッコ"
		CASE 1
			S_SLANGWORD_Semen += "エッチな汁"
		CASEELSE
			S_SLANGWORD_Semen += "赤ちゃんの素"
	ENDSELECT
ELSEIF TALENT:淫乱 > 0
	SELECTCASE RAND:5
		CASE 4
			S_SLANGWORD_Semen += "おちんぽミルク"
		CASE 3
			S_SLANGWORD_Semen += "雄ミルク"
		CASEELSE
			SELECTCASE RAND:3
				CASE 2
					S_SLANGWORD_Semen += "孕ませ"
				CASE 1
					S_SLANGWORD_Semen += "ザー"
				CASEELSE
					S_SLANGWORD_Semen += "子種"
			ENDSELECT
			S_SLANGWORD_Semen += "汁"
	ENDSELECT
ELSE
	SELECTCASE RAND:3
		CASE 2
			S_SLANGWORD_Semen += "スペルマ"
		CASE 1
			S_SLANGWORD_Semen += "ザーメン"
		CASEELSE
			S_SLANGWORD_Semen += "精液"
	ENDSELECT
ENDIF


;合成して出来上がった淫語を返す
RETURNF S_SLANGWORD_Semen





