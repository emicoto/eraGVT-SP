;**********************************************************
;幽閉コマンド
;PRISON_COM200
;Vに産卵
;**********************************************************
@PRISON_COM200
#DIM CCOUNT
VARSET LOCAL

;取得する珠
;LOCAL:0 快Ｃ
;LOCAL:1 快Ｖ
;LOCAL:2 快Ａ
;LOCAL:3 快Ｂ
;LOCAL:4 潤滑 潤滑は未使用
;LOCAL:5 恭順
;LOCAL:6 習得
;LOCAL:7 欲情
;LOCAL:8 屈服
;LOCAL:9 恥情
;LOCAL:10 苦痛
;LOCAL:11 恐怖

;経験数の上量値記録用
;LOCAL:120 V経験
;LOCAL:121 A経験
;LOCAL:123 精液経験
;LOCAL:124 フェラ経験
;LOCAL:130 露出快楽経験,
;LOCAL:131 奉仕快楽経験,
;LOCAL:132 苦痛快楽経験,
;LOCAL:140 自慰経験,
;LOCAL:150 異常経験
;LOCAL:151 Ｖ拡張経験
;LOCAL:152 Ａ拡張経験
;LOCAL:153 射精経験
;LOCAL:154 噴乳経験
;LOCAL:155 放尿経験
;LOCAL:156 寄生経験
;LOCAL:157 出産経験

;汚染度
CFLAG:30 += 10 + RAND:10

;近親交配経験
SIF INCEST_F(TARGET, CFLAG:20 * 100 + CFLAG:21, 1) > 0
	LOCAL:141 += 1

;快V
IF ABL:Ｖ感覚 == 0
	LOCAL:120 = 5
	LOCAL:151 = 1
	LOCAL:1 = 300
ELSEIF ABL:Ｖ感覚 == 1
	LOCAL:120 = 8
	LOCAL:151 = 1
	LOCAL:1 = 450
ELSEIF ABL:Ｖ感覚 == 2
	LOCAL:120 = 11
	LOCAL:151 = 2
	LOCAL:1 = 600
ELSEIF ABL:Ｖ感覚 == 3
	LOCAL:120 = 15
	LOCAL:151 = 2
	LOCAL:1 = 800
ELSEIF ABL:Ｖ感覚 == 4
	LOCAL:120 = 20
	LOCAL:151 = 4
	LOCAL:1 = 1200
ELSEIF ABL:Ｖ感覚 >= 5
	LOCAL:120 = 30
	LOCAL:151 = 8
	LOCAL:1 = 2000
ENDIF
;出産経験が少ない場合は快Vが入りにくい
IF EXP:出産経験 < 3
	TIMES LOCAL:1, 0.00
ELSEIF EXP:出産経験 < 5
	TIMES LOCAL:1, 0.50
ELSEIF EXP:出産経験 < 8
	TIMES LOCAL:1, 0.80
ELSE
ENDIF

;恭順
;出産経験が多いと恭順が入る
IF EXP:出産経験 < 3
ELSEIF EXP:出産経験 < 5
	LOCAL:5 = 200
ELSEIF EXP:出産経験 < 8
	LOCAL:5 = 1000
ELSE
	LOCAL:5 = 5000
ENDIF

;屈服
;出産経験に応じて
IF EXP:出産経験 < 3
	LOCAL:8 = 500
ELSEIF EXP:出産経験 < 5
	LOCAL:8 = 1000
ELSEIF EXP:出産経験 < 8
	LOCAL:8 = 2000
ELSE
	LOCAL:8 = 5000
ENDIF

;恥情
;出産経験が低いと恥情が入る
IF EXP:出産経験 < 3
	LOCAL:9 = 800
ELSEIF EXP:出産経験 < 5
	LOCAL:9 = 400
ELSEIF EXP:出産経験 < 8
	LOCAL:9 = 200
ELSE
	LOCAL:9 = 50
ENDIF

;苦痛
;出産経験が低いと入る
IF EXP:出産経験 < 3
	LOCAL:10 = 1500
ELSEIF EXP:出産経験 < 5
	LOCAL:10 = 1000
ELSEIF EXP:出産経験 < 8
	LOCAL:10 = 500
ELSE
	LOCAL:10 = 200
ENDIF

;恐怖
;出産経験が低いと入る
IF EXP:出産経験 < 3
	LOCAL:11 = 1500
ELSEIF EXP:出産経験 < 5
	LOCAL:11 = 1000
ELSEIF EXP:出産経験 < 8
	LOCAL:11 = 500
ELSE
	LOCAL:11 = 200
ENDIF

;地の文：Vに産卵
CALL MESSAGE_PRISON_COM_200

;習得基本値
LOCAL:6 += 0

;欲情基本値
LOCAL:7 += 0

;屈服基本値
LOCAL:8 += 500

;恥情基本値
LOCAL:9 += 0

;苦痛基本値
LOCAL:10 += 500

;恐怖基本値
LOCAL:11 += 500

;初めてのV出産の場合、異常経験が入る
IF CFLAG:204 == 0
	CFLAG:204 = 1
	LOCAL:150 = 1
ENDIF

;幽閉中パラメータ変動共通処理へ
CALL COMMON_PRISON, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11

;拡張度による拡張経験
CALL PRISON_GAPING, LOCAL:120, LOCAL:121, LOCAL:151, LOCAL:152
LOCAL:151 += RESULT:0
LOCAL:152 += RESULT:1

;経験数の増加
CALL COMMON_PRISON_EXP_SH, LOCAL:120, LOCAL:121, LOCAL:151, LOCAL:152
LOCAL:120 = RESULT:0
LOCAL:121 = RESULT:1
LOCAL:151 = RESULT:2
LOCAL:152 = RESULT:3

;表示と増加の処理
FOR CCOUNT, 100, 200
	CALL COMMON_PRISON_EXP, CCOUNT, LOCAL:CCOUNT
NEXT

PRINTL 

;珠の取得へ
CALL _ABLUP, 1
;妊娠
SIF SHIELD:1 <= 0
	TRYCALL NINSIN_HANTEI,50,4000000
