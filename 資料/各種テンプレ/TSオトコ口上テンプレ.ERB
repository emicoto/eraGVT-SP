	SELECTCASE RAND:2
		CASE 0
			PRINTFORML 
		CASE 1
			PRINTFORML 
	ENDSELECT
;触手の虜取得後、1/3の確率で
ELSEIF TALENT:触手の虜 && RAND:3 == 0
	SELECTCASE RAND:2
		CASE 0
			PRINTFORML 
		CASE 1
			PRINTFORML 
	ENDSELECT
;快楽刻印（高いほど発生しやすい）
ELSEIF MARK:快楽刻印 > RAND:5
	SELECTCASE RAND:2
		CASE 0
			PRINTFORML 
		CASE 1
			PRINTFORML 
	ENDSELECT
;苦痛、恐怖刻印（平均が高いほど発生しやすい）
ELSEIF (MARK:苦痛刻印 + MARK:恐怖刻印)/2 > RAND:4
	SELECTCASE RAND:2
		CASE 0
			PRINTFORML 
		CASE 1
			PRINTFORML 
	ENDSELECT
;通常
ELSE
	SELECTCASE RAND:8
		CASE 0
			PRINTFORML 
		CASE 1
			PRINTFORML 
		CASE 2
			PRINTFORML 
		CASE 3
			PRINTFORML 
		CASE 4
			PRINTFORML 
		CASE 5
			PRINTFORML 
		CASE 6
			PRINTFORML 
		CASE 7
			PRINTFORML 
	ENDSELECT

ENDIF

